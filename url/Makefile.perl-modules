perl_modules-touch:
	for i in $(perl_modules_src_0) \
        $(perl_modules_src_1) $(perl_modules_src_2) \
        $(perl_modules_src_3) $(perl_modules_src_4) \
        $(perl_modules_src_5) $(perl_modules_src_dbi); \
        do \
        touch /opt/moto/.sb/$$i; \
        done

perl_modules_clean:
	for i in $(perl_modules_src_0) \
        $(perl_modules_src_1) $(perl_modules_src_2) \
        $(perl_modules_src_3) $(perl_modules_src_4) \
        $(perl_modules_src_5) $(perl_modules_src_dbi); \
        do \
        rm  /opt/moto/.sb/$$i; \
        done


perl_modules_dbi-touch:
	for i in $(perl_modules_src_dbi); \
        do \
        touch /opt/moto/.sb/$$i; \
        done

