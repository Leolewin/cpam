      <init link-src="/sbin/rc1.d/K268TWWnetsnmp57"
        link-dest="/sbin/init.d/TWWnetsnmp57"/>

      <config>/sbin/init.d/TWWnetsnmp57</config>
      <config upgrade="1">etc/snmp.conf</config>
      <config upgrade="1">etc/snmp.local.conf</config>
      <config upgrade="1">etc/snmpapp.conf</config>
      <config upgrade="1">etc/snmpapp.local.conf</config>
      <config>/etc${INSTALL_PREFIX}/snmpd.conf</config>
      <config>/etc${INSTALL_PREFIX}/snmpd.local.conf</config>

      <subpkg type="runtime">
