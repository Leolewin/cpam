    <configure>
<![CDATA[
case "${SYSTYPE}" in
*-aix*)
  CFLAGS="-O2 -pthread -mcpu=powerpc"
  CXXFLAGS="-O2 -pthread -mcpu=powerpc"
  LDFLAGS="-Wl,-brtl -Wl,-blibpath:/usr/lib"
  CC_LD_RT="-Wl,-blibpath:"

  _gcc_rt_lib="${SB_VAR_GCC_RT_LIB}/pthread" ;;
ia64*-hpux*)
  CC=cc CFLAGS="-z -mt +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-AA -z -mt +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CC_LD_RT="-Wl,+b,"

  ARGS="--x-includes=/usr/include/X11R6 \
--x-libraries=/usr/lib/X11R6" ;;
*-hpux*)
  CC_LD_RT="-Wl,+s,+b,"

  ARGS="--x-includes=/usr/include/X11R6 \
--x-libraries=/usr/lib/X11R6" ;;
*-irix*)
  CFLAGS="-O2 -mips4" CXXFLAGS="-O2 -mips4"
  CC_LD_RT="-Wl,-rpath," ;;
i?86*-linux*)
  CFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CXXFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CC_LD_RT="-Wl,-rpath,"

  _aiksaurus_lib=${SB_VAR_AIKSAURUS_LIB}
  _aspell_lib="${SB_VAR_ASPELL_LIB}"
  _libqt_inc="${SB_VAR_LIBQT_INC}"
  _libqt_lib="${SB_VAR_LIBQT_LIB}"
  _libqt_uic="${SB_VAR_LIBQT}/bin/uic" ;;
*-linux*)
  CC_LD_RT="-Wl,-rpath,"

  _aiksaurus_lib=${SB_VAR_AIKSAURUS_LIB}
  _aspell_lib="${SB_VAR_ASPELL_LIB}"
  _libqt_inc="${SB_VAR_LIBQT_INC}"
  _libqt_lib="${SB_VAR_LIBQT_LIB}"
  _libqt_uic="${SB_VAR_LIBQT}/bin/uic" ;;
*-osf*)
  # -nodtk so that the C++ compiler sees the same headers
  # as the compiler
  CC=cc CFLAGS="-O2 -nodtk -arch ev5 -msym -readonly_strings"
  CXX=cxx CXXFLAGS="-O2 -arch ev5 -model ansi -msym \
-readonly_strings -timplicit_local -D__USE_STD_IOSTREAM"
  CC_LD_RT="-Wl,-rpath,"

  ARGS="ac_cv_func_setenv=no ac_cv_header_inttypes_h=yes \
gl_cv_header_inttypes_h=yes" ;;
i?86*-solaris*)
  CFLAGS="-O2 -mtune=pentium4 -march=pentium4"
  CXXFLAGS="-O2 -mtune=pentium4 -march=pentium4"
  CC_LD_RT="-R" ;;
*-solaris*)
  CFLAGS="-O2 -mcpu=ultrasparc -mvis"
  CXXFLAGS="-O2 -mcpu=ultrasparc -mvis"
  CC_LD_RT="-R" ;;
esac

${SHELL} ./configure CPPFLAGS="-I${SB_VAR_AIKSAURUS_INC} \
${SB_VAR_ASPELL+-I${SB_VAR_ASPELL_INC}} \
${SB_VAR_XPM+-I${SB_VAR_XPM_INC}} \
${SB_VAR_LIBZ+-I${SB_VAR_LIBZ_INC}} \
${SB_VAR_LIBICONV+-I${SB_VAR_LIBICONV_INC}} \
${SB_VAR_GETTEXT+-I${SB_VAR_GETTEXT_INC}} \
${CPPFLAGS+${CPPFLAGS}}" \
CC="${CC:-gcc}" CFLAGS="${CFLAGS:--O2}" \
CXX="${CXX:-g++}" CXXFLAGS="${CXXFLAGS:--O2}" \
LDFLAGS="${SB_VAR_GCC_RT+-L${_gcc_rt_lib:-${SB_VAR_GCC_RT_LIB}}} \
${SB_VAR_JPEG+-L${SB_VAR_JPEG_LIB}} \
${SB_VAR_LIBZ+-L${SB_VAR_LIBZ_LIB}} \
${SB_VAR_XPM+-L${SB_VAR_XPM_LIB}} \
-L${_aiksaurus_lib:-${SB_VAR_AIKSAURUS_LIB}${SB_VAR_GCC_RT+/${_gcc_subdir}}} \
${SB_VAR_ASPELL+-L${_aspell_lib:-${SB_VAR_ASPELL_LIB}\
${SB_VAR_GCC_RT+/${_gcc_subdir}}}} \
${SB_VAR_LIBICONV+-L${SB_VAR_LIBICONV_LIB}} \
${SB_VAR_GETTEXT+-L${SB_VAR_GETTEXT_LIB}} \
${CC_LD_RT}${SB_VAR_GCC_RT+${_gcc_rt_lib:-${SB_VAR_GCC_RT_LIB}}}:\
${_aiksaurus_lib:-${SB_VAR_AIKSAURUS_LIB}\
${SB_VAR_GCC_RT+/${_gcc_subdir}}}:\
${_libqt_lib:-${SB_VAR_LIBQT_LIB}\
${SB_VAR_GCC_RT+/${_gcc_subdir}}}\
${SB_VAR_JPEG+:${SB_VAR_JPEG_LIB}}\
${SB_VAR_XPM+:${SB_VAR_XPM_LIB}}\
${SB_VAR_LIBTIFF+:${SB_VAR_LIBTIFF_LIB}}\
${SB_VAR_LIBZ+:${SB_VAR_LIBZ_LIB}}\
${SB_VAR_ASPELL+:${_aspell_lib:-${SB_VAR_ASPELL_LIB}\
${SB_VAR_GCC_RT+/${_gcc_subdir}}}}\
${SB_VAR_LIBICONV+:${SB_VAR_LIBICONV_LIB}}\
${SB_VAR_GETTEXT+:${SB_VAR_GETTEXT_LIB}} ${LDFLAGS+${LDFLAGS}}" \
PKG_CONFIG_PATH="${SB_VAR_LIBQT+${_libqt_lib:-${SB_VAR_LIBQT_LIB}\
${SB_VAR_GCC_RT+/${_gcc_subdir}}}/pkgconfig}" \
M4="${SB_VAR_M4-/usr}/bin/${SB_VAR_M4+g}m4" \
${SB_VAR_LIBICONV+--with-libiconv-prefix=${SB_VAR_LIBICONV}} \
${SB_VAR_GETTEXT+--with-libintl-prefix=${SB_VAR_GETTEXT}} \
--disable-dependency-tracking --with-pspell \
--with-frontend=qt4 --disable-pch \
--with-qt4-includes=${_libqt_inc:-${SB_VAR_LIBQT_INC}\
${SB_VAR_GCC_RT+/${_gcc_subdir}}} \
--with-qt4-libraries=${_libqt_lib:-${SB_VAR_LIBQT_LIB}\
${SB_VAR_GCC_RT+/${_gcc_subdir}}} \
qt_cv_path_uic=${_libqt_uic:-${SB_VAR_LIBQT}/bin\
${SB_VAR_GCC_RT+/${_gcc_subdir}}/uic} \
--datadir=${_datadir} ${ARGS+${ARGS}} \
--mandir=${SB_INSTALL_PREFIX}/man --prefix=${SB_INSTALL_PREFIX}

gmake configure

# Use our Python
${SB_PATH_SED} -i -e "\
s!python -tt!${SB_VAR_PYTHON}/bin/python -tt!" src/support/os.cpp
