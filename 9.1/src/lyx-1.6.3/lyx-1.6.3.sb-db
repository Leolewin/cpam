lib/scripts/TeXFiles.py lib/scripts/convertDefault.py \
lib/scripts/fig2pdftex.py lib/scripts/legacy_lyxpreview2ppm.py \
lib/scripts/lyxpreview_tools.py lib/scripts/layout2layout.py \
lib/scripts/fen2ascii.py lib/scripts/date.py lib/scripts/fig2pstex.py \
lib/scripts/csv2lyx.py lib/scripts/ext_copy.py \
