  <package name="setuptools" version="0.6" revision="9">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>setuptools - Python distutils enhancements</title>
      <vendor>Phillip J. Eby</vendor>

      <description>
setuptools is a collection of enhancements to the Python distutils
that allow you to more easily build and distribute Python packages,
especially ones that have dependencies on other packages.
      </description>

      <install-name>setuptools06</install-name>

      <pkgname-base>TWWsetptl06</pkgname-base>

      <version>0.6</version>
      <revision>10</revision>

      <variable>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWpython26"
          subpkg="runtime">v&gt;=2.6.7</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="0b792fd55ba8d05e66984de0d941a138"
          checksum-type="md5" size="10119"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <changelog>
        <change date="2011 October 13" revision="10"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add modules for Python 2.6.7, 2.7.2</para></item>
        </change>

        <change date="2011 October 12" revision="9"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
