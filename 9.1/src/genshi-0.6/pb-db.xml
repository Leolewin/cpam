    <package-manager name="depot">
      <category>Applications/Internet</category>
      <title>Genshi HTML template system</title>
      <vendor>Edgewall software</vendor>

      <description>
Genshi is a Python library that provides an integrated set of
components for parsing, generating, and processing HTML, XML or other
textual content for output generation on the web.
      </description>

      <install-name>genshi06</install-name>

      <pkgname-base>TWWgenshi06</pkgname-base>

      <version>0.6</version>
      <revision>3</revision>

      <variable>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWpython26"
          subpkg="runtime">v&gt;=2.6.7</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="bde8ab6c15e72c27f3db93651001c8e6"
          checksum-type="md5" size="10031"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <changelog>
        <change date="2012 January 18" revision="3"
        author="Gary V. Vaughan" email="gary@thewrittenword.com">
          <item><para>Update python dependency from 2.5.2 to
2.6.7</para></item>
        </change>

        <change date="2010 March 2" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Fix quoting bug in preremove
script</para></item>
        </change>
      </changelog>
    </package-manager>

