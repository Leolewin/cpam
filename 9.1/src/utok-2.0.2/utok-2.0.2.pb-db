<?xml version="1.0"?>
<packages>
  <package name="utok" version="2.0.2" revision="2">
    <package-manager name="depot">
      <category>Applications/System</category>
      <title>Unique TOKens</title>
      <vendor>Sven Heinicke</vendor>

      <description>
utok is a utility that is designed to keep environmental variables
containing lists cleaner. With it one can easily add or remove tokens
with a PATH. It prevents items from showing up multiple times in
delineated list, and allows items to be easily removed.
      </description>

      <install-name>utok</install-name>

      <pkgname-base>TWWutok</pkgname-base>

      <version>2.0.2</version>
      <revision>3</revision>

      <changelog>
        <change date="2006 May 15" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Include missing <command>info/dir</command>
file</para></item>
        </change>
      </changelog>
    </package-manager>

