<?xml version="1.0"?>
<packages>
  <package name="mercurial" version="1.9.2">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>Mercurial -- a distributed SCM</title>
      <vendor>Matt Mackall, Mercurial Developers</vendor>

      <description attribution="The Fedora Project">
Mercurial is a fast, lightweight source control management system
designed for efficient handling of very large distributed projects.
      </description>

      <install-name>mercurial19</install-name>

      <pkgname-base>TWWmercurial19</pkgname-base>

      <version>1.9.2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWpython26"
          subpkg="runtime">v&gt;=2.6.7</depend>
