<?xml version="1.0"?>
<packages>
  <package name="glibmm" version="2.24.2">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>C++ interface to Glib library</title>
      <vendor>glibmm developers</vendor>

      <description attribution="freshmeat.net">
glibmm is a set of C++ bindings for glib, including cross- platform
APIs such as a std::string-like UTF8 string class, string utility
methods, such as a text encoding converter API, file access, and
threads.
      </description>

      <install-name>libglibmm224</install-name>

      <pkgname-base>TWWlibglibmm224</pkgname-base>

      <version>2.24.2</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          systype="hppa*-hpux*"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWlibglib226"
          subpkg="runtime">v&gt;=2.26.1 r&gt;=3</depend>
        <depend pkgname-base="TWWlibsigcpp22"
          subpkg="runtime">v&gt;=2.2.10 r&gt;=2</depend>
      </subpkg>

      <replace pkgname-base="TWWlibglibmm216"
        install-name="libglibmm216"/>
      <replace pkgname-base="TWWlibglibmm212"
        install-name="libglibmm212"/>

      <changelog>
        <change date="2012 May 11" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add 64bit libraries for GCC</para></item>
          <item><para>Add native 64bit libraries</para></item>
        </change>
      </changelog>
    </package-manager>

