<?xml version="1.0"?>
<packages>
  <package name="xulrunner" version="1.9.2.25">
    <package-manager name="depot">
      <category>Applications/Internet</category>
      <title>Xulrunner, mozilla runtime package</title>
      <vendor>mozilla.org</vendor>

      <description>
XULRunner provides the XUL Runtime environment for Gecko applications.
      </description>

      <install-name>xulrunner192</install-name>

      <pkgname-base>TWWxulrunner192</pkgname-base>

      <version>1.9.2.25</version>
      <revision>2</revision>

      <config>lib/greprefs/all.js</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibcairo110"
          subpkg="runtime">v&gt;=1.10.2</depend>
        <depend pkgname-base="TWWlibgtk222"
          subpkg="runtime">v&gt;=2.22.1</depend>
        <depend pkgname-base="TWWlibidl08"
          subpkg="runtime">v&gt;=0.8.14</depend>
        <depend pkgname-base="TWWlibevent14"
          subpkg="runtime">v&gt;=1.4.14</depend>
      </subpkg>

      <replace pkgname-base="TWWxulrunner191"
        install-name="xulrunner191"/>
      <replace pkgname-base="TWWxulrunner190"
        install-name="xulrunner190"/>

      <changelog>
        <change date="2012 March 11" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update cairo dependency from 1.8.8 to
1.10.2</para></item>
          <item><para>Update gtk dependency from 2.12.11 to
2.22.1</para></item>
          <item><para>Update libIDL dependency from 0.8.11 to
0.8.14</para></item>
        </change>
      </changelog>
    </package-manager>

