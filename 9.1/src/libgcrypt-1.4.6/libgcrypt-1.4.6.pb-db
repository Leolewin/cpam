<?xml version="1.0"?>
<packages>
  <package name="libgcrypt" version="1.4.6">
    <package-manager name="depot">
      <category>Development/Libraries</category>
      <title>General-purpose crypto library</title>
      <vendor>g10 Code GmbH</vendor>

      <description>
Libgcrypt is a general purpose crypto library based on the code used
in GnuPG.
      </description>

      <install-name>libgcrypt14</install-name>

      <pkgname-base>TWWlibgcrypt14</pkgname-base>

      <version>1.4.6</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgpgerror110"
          subpkg="runtime">v&gt;=1.10</depend>
        <depend pkgname-base="TWWprngd"
          systype="*-hpux11.[01]*"
          subpkg="conf">v&gt;=0.9.25</depend>
        <depend pkgname-base="TWWprngd"
          systype="*-hpux11.[01]*"
          subpkg="runtime">v&gt;=0.9.25</depend>
      </subpkg>

      <replace pkgname-base="TWWgcrypt12"
        install-name="libgcrypt12"/>
    </package-manager>

    <package-manager name="inst">
      <category>Development/Libraries</category>
      <title>General-purpose crypto library</title>
      <vendor>g10 Code GmbH</vendor>

      <description>
Libgcrypt is a general purpose crypto library based on the code used
in GnuPG.
      </description>

      <install-name>libgcrypt14</install-name>

      <pkgname-base>TWWlibgcrypt14</pkgname-base>

      <version>2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgpgerror110"
          subpkg="runtime">v&gt;=1</depend>
        <depend pkgname-base="TWWprngd"
          subpkg="conf">v&gt;=4</depend>
        <depend pkgname-base="TWWprngd"
          subpkg="runtime">v&gt;=4</depend>
      </subpkg>

      <replace pkgname-base="TWWlibgcrypt12"
        install-name="libgcrypt12"/>
    </package-manager>

    <package-manager name="lpp">
      <category>Development/Libraries</category>
      <title>General-purpose crypto library</title>
      <vendor>g10 Code GmbH</vendor>

      <description>
Libgcrypt is a general purpose crypto library based on the code used
in GnuPG.
      </description>

      <install-name>libgcrypt14</install-name>

      <pkgname-base>TWW.libgcrypt14</pkgname-base>

      <version>1.4.6</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWW.libgpgerror110"
          subpkg="runtime">v&gt;=1.10.0.1</depend>
      </subpkg>

      <replace pkgname-base="TWW.libgcrypt12"
        install-name="libgcrypt12"/>
    </package-manager>

    <package-manager name="pkgadd">
      <category>Development/Libraries</category>
      <title>General-purpose crypto library</title>
      <vendor>g10 Code GmbH</vendor>

      <description>
Libgcrypt is a general purpose crypto library based on the code used
in GnuPG.
      </description>

      <install-name>libgcrypt14</install-name>

      <pkgname-base>TWWlibgcrypt14</pkgname-base>

      <version>1.4.6</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgpgerror110"
          title="GnuPG components error library (libgpg-error)"
          subpkg="runtime">v&gt;=1.10</depend>
      </subpkg>

      <replace pkgname-base="TWWlibgcrypt12"
        install-name="libgcrypt12"/>
    </package-manager>

    <package-manager name="rpm4">
      <category>Development/Libraries</category>
      <title>General-purpose crypto library</title>
      <vendor>g10 Code GmbH</vendor>

      <description>
Libgcrypt is a general purpose crypto library based on the code used
in GnuPG.
      </description>

      <install-name>libgcrypt14</install-name>

      <pkgname-base>TWWlibgcrypt14</pkgname-base>

      <version>1.4.6</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgpgerror110"
          subpkg="runtime">v&gt;=1.10</depend>
        <depend pkgname-base="TWWprngd"
          systype="*-hpux11.[01]*|*-irix*|*-osf*"
          subpkg="conf">v&gt;=0.9.25</depend>
        <depend pkgname-base="TWWprngd"
          systype="*-hpux11.[01]*|*-irix*|*-osf*"
          subpkg="runtime">v&gt;=0.9.25</depend>
      </subpkg>

      <replace pkgname-base="TWWlibgcrypt12"
        install-name="libgcrypt12"/>
    </package-manager>

    <package-manager name="setld">
      <category>Development/Libraries</category>
      <title>General-purpose crypto library</title>
      <vendor>g10 Code GmbH</vendor>

      <description>
Libgcrypt is a general purpose crypto library based on the code used
in GnuPG.
      </description>

      <install-name>libgcrypt14</install-name>

      <pkgname-base>TWWLIBGCRYPT14</pkgname-base>

      <version>1.4.6</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWLIBGPGERROR110"
          subpkg="runtime">v&gt;=1.10</depend>
        <depend pkgname-base="TWWPRNGD"
          subpkg="conf">v&gt;=0.9.25</depend>
        <depend pkgname-base="TWWPRNGD"
          subpkg="runtime">v&gt;=0.9.25</depend>
      </subpkg>

      <replace pkgname-base="TWWLIBGCRYPT12"
        install-name="libgcrypt12"/>
    </package-manager>
  </package>
</packages>
