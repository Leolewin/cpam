<?xml version="1.0"?>
<packages>
  <package name="logrotate" version="3.6.6" revision="4">
    <package-manager name="depot">
      <category>Applications/System</category>
      <title>Log Rotation Tool</title>
      <vendor>Redhat, Inc.</vendor>

      <description>
The logrotate utility is designed to simplify the administration of
log files on a system which generates a lot of log files. Logrotate
allows for the automatic rotation compression, removal and mailing of
log files. Logrotate can be set to handle a log file daily, weekly,
monthly or when the log file gets to a certain size.
      </description>

      <install-name>logrotate36</install-name>

      <pkgname-base>TWWlogrot36</pkgname-base>

      <version>3.6.6</version>
      <revision>5</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgzip14"
          subpkg="runtime">v&gt;=1.4</depend>
        <depend pkgname-base="TWWlibpopt116"
          subpkg="runtime">v&gt;=1.16</depend>
      </subpkg>

      <changelog>
        <change date="2011 October 13" revision="5"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Updated gzip dependency from 1.3.12 to
1.4</para></item>
          <item><para>Updated popt dependency from 1.14 to
1.16</para></item>
        </change>

        <change date="2009 August 11" revision="4"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gzip dependency from 1.3.5 to
1.3.12</para></item>
          <item><para>Update popt dependency from 1.7 to
1.14</para></item>
        </change>

        <change date="2004 April 13" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Fix problem when olddir is a relative 
path</para></item>
        </change>
      </changelog>
    </package-manager>

