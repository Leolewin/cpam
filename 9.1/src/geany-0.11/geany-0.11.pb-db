<?xml version="1.0"?>
<packages>
  <package name="geany" version="0.11" revision="2">
    <package-manager name="depot">
      <category>Applications/Editors</category>
      <title>GNU Emacs Editor</title>
      <vendor>Enrico Trger, et., al.</vendor>

      <description>
Geany is a small and lightweight integrated development environment.
It was developed to provide a small and fast IDE, which has only a few
dependencies from other packages.

Basic features:
  * syntax highlighting
  * code folding
  * code completion
  * auto completion of often used constructs like if, for and while
  * auto completion of XML and HTML tags
  * call tips
  * many supported filetypes like C, Java, PHP, HTML, Python,
    Perl, Pascal (full list)
  * symbol lists
      </description>

      <install-name>geany01</install-name>

      <pkgname-base>TWWgeany01</pkgname-base>

      <version>0.11</version>
      <revision>4</revision>

      <variable>
        <path name="GTKTHEMES_PATH"
          install-name="gtkthemes222"/>
        <path name="XDGDATA_DATADIR_PATH"
          install-name="xdgdata12">share</path>
      </variable>

      <wrapper name="geany" path="pkg/geany-wrapper.c"
        checksum="2eed4700b0dd6fbb5d6585811d5c6da5"
        checksum-type="md5" size="1951"/>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgtk222"
          subpkg="runtime">v&gt;=2.22.1</depend>
      </subpkg>

      <changelog>
        <change date="2012 January 30" revision="4"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gtk dependency from 2.12.11 to
2.22.1</para></item>
        </change>

        <change date="2010 February 10" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Use gtkthemes-2.12, not 2.10</para></item>
        </change>

        <change date="2009 November 26" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gtk dependency from 2.10.14 to
2.12.11</para></item>
        </change>
      </changelog>
    </package-manager>

