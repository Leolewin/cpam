      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibiconv113"
          subpkg="runtime">v&gt;=1</depend>
        <depend pkgname-base="TWWlibopenssl10"
          subpkg="runtime">v&gt;=1</depend>
      </subpkg>

      <replace pkgname-base="TWWrdesktop15"
        install-name="rdesktop15"/>

      <changelog>
        <change date="2012 January 25" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
