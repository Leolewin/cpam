<?xml version="1.0"?>
<packages>
  <package name="rdesktop" version="1.6.0" revision="2">
    <package-manager name="depot">
      <category>User Interface/Desktops</category>
      <title>rdesktop Remote Desktop Protocol (RDP) client</title>
      <vendor>rdesktop developers</vendor>

      <description>
rdesktop is a client for Microsoft Windows NT Terminal Server, Windows
2000 Terminal Services, Windows 2003 Terminal Services/Remote Desktop,
Windows XP Remote Desktop, and possibly other Terminal Services
products. rdesktop currently implements the RDP version 4 and 5
protocols.
      </description>

      <install-name>rdesktop16</install-name>

      <pkgname-base>TWWrdesk16</pkgname-base>

      <version>1.6.0</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibiconv113"
          subpkg="runtime">v&gt;=1.13.1</depend>
        <depend pkgname-base="TWWlibopenssl10"
          subpkg="runtime">v&gt;=1.0.0e</depend>
      </subpkg>

      <replace pkgname-base="TWWrdesk15"
        install-name="rdesktop15"/>
    </package-manager>

