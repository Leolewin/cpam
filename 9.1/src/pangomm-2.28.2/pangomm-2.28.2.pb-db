<?xml version="1.0"?>
<packages>
  <package name="pangomm" version="2.28.2">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>C++ interface to pango library</title>
      <vendor>gtkmm developers</vendor>

      <description>
pangomm provides a C++ interface to the Pango library. Highlights
include typesafe callbacks, widgets extensible via inheritance and a
comprehensive set of widget classes that can be freely combined to
quickly create complex user interfaces.
      </description>

      <install-name>libpangomm228</install-name>

      <pkgname-base>TWWlibpangomm228</pkgname-base>

      <version>2.28.2</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          systype="hppa*-hpux*"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWlibcairomm110"
          subpkg="runtime">v&gt;=1.10.0 r&gt;=2</depend>
        <depend pkgname-base="TWWlibglibmm224"
          subpkg="runtime">v&gt;=2.24.2 r&gt;=2</depend>
      </subpkg>

      <changelog>
        <change date="2012 May 9" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add 64bit libraries for GCC</para></item>
          <item><para>Add native 64bit libraries</para></item>
        </change>
      </changelog>
    </package-manager>

