  <package name="bluefish" version="2.0" revision="2">
    <package-manager name="depot">
      <category>Applications/Editors</category>
      <title>Bluefish - GTK2 web development application</title>
      <vendor>Olivier Sessink, Daniel Leidert, et.al.</vendor>

      <description attribution="The Fedora Project">
Bluefish is a powerful editor for experienced web designers and
programmers. Bluefish supports many programming and markup languages,
but it focuses on editing dynamic and interactive websites.
      </description>

      <install-name>bluefish20</install-name>

      <pkgname-base>TWWbluefish20</pkgname-base>

      <version>2.0</version>
      <revision>2</revision>

      <variable>
        <path name="GNOME_PATH"
          install-name="gnome232"/>
        <path name="GNOME_DATADIR_PATH"
          install-name="gnome232">share</path>
        <path name="GNOME_SYSCONFDIR_PATH"
          install-name="gnome232">etc</path>
        <path name="BLUEFISH_DATADIR_PATH"
          install-name="bluefish20">share</path>
        <path name="GTKTHEMES_PATH"
          install-name="gtkthemes222"/>
        <path name="XDGDATA_DATADIR_PATH"
          install-name="xdgdata12">share</path>
      </variable>

      <wrapper name="bluefish" path="pkg/bluefish-wrapper.c"
        checksum="fbba44339e873d116f085216e633e730"
        checksum-type="md5" size="2266"/>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgnome232"
          subpkg="runtime">v&gt;=2.32</depend>
        <depend pkgname-base="TWWlibenchant16"
          subpkg="runtime">v&gt;=1.6.0</depend>
        <depend pkgname-base="TWWlibxml27"
          subpkg="runtime">v&gt;=2.7.8</depend>
        <depend pkgname-base="TWWlibgtk222"
          subpkg="runtime">v&gt;=2.22.1</depend>
        <depend pkgname-base="TWWlibpcre811"
          subpkg="runtime">v&gt;=8.11</depend>
        <depend pkgname-base="TWWxdgdata12"
          subpkg="runtime">v&gt;=1.2</depend>
      </subpkg>

      <changelog>
        <change date="2012 January 24" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update enchant dependency from 1.5.0 to
1.6.0</para></item>
