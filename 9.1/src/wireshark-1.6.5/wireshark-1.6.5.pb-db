          subpkg="runtime">v&gt;=5.1.4</depend>
        <depend pkgname-base="TWWlibpcre811"
          subpkg="runtime">v&gt;=8.11</depend>
        <depend pkgname-base="TWWxdgdata12"
          subpkg="runtime">v&gt;=1.2</depend>
      </subpkg>

      <replace pkgname-base="TWWwires14"
        install-name="wireshark14"/>
      <replace pkgname-base="TWWwires12"
        install-name="wireshark12"/>
