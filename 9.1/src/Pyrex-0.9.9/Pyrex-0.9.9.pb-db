  <package name="Pyrex" version="0.9.9">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>Pyrex - a Language for Writing Python Extension Modules</title>
      <vendor>Greg Ewing</vendor>

      <description>
Pyrex is a language specially designed for writing Python extension
modules. It's designed to bridge the gap between the nice, high-level,
easy-to-use world of Python and the messy, low-level world of C.
      </description>

      <install-name>pyrex09</install-name>

      <pkgname-base>TWWpyrex09</pkgname-base>

      <version>0.9.9</version>
      <revision>1</revision>

      <variable>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWpython26"
          subpkg="runtime">v&gt;=2.6.7</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
