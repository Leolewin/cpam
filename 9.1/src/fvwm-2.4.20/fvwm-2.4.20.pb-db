<?xml version="1.0"?>
<packages>
  <package name="fvwm" version="2.4.20" revision="2">
    <package-manager name="depot">
      <category>User Interface/Desktops</category>
      <title>FVWM Window Manager</title>
      <vendor>The fvwm-workers</vendor>

      <description>
Fvwm is a window manager for X11. It is designed to minimize memory
consumption, provide a 3D look to window frames, and a virtual
desktop.
      </description>

      <install-name>fvwm24</install-name>

      <pkgname-base>TWWfvwm24</pkgname-base>

      <version>2.4.20</version>
      <revision>2</revision>

      <config>etc/system.fvwm2rc</config>

      <files>
        <file>/etc/dt/config/C/Xresources.d/Xresources.fvwm.tww-2.4</file>
        <file>/etc/dt/config/Xinitrc.fvwm.tww-2.4</file>
        <file>/etc/dt/config/Xsession.fvwm.tww-2.4</file>
        <file>/etc/dt/appconfig/icons/C/FVWMlogo24.pm</file>
      </files>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgtk12"
          subpkg="runtime">v&gt;=1.2.10</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="runtime">v&gt;=5.12.2</depend>
        <depend pkgname-base="TWWxpm"
          subpkg="runtime">v&gt;=3.4k</depend>
      </subpkg>

      <replace pkgname-base="TWWfvwm22"
        install-name="fvwm22"/>

      <changelog>
        <change date="2012 January 30" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update perl dependency from 5.8.8 yo
5.12.2</para></item>
        </change>
      </changelog>
    </package-manager>

