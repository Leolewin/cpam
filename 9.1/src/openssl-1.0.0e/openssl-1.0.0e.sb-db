    <dependencies>
      <depend program="perl" add-path="prepend"
        type="build">v==5.12.2</depend>
      <depend program="prngd" var="PRNGD" install-name="prngd"
        systype="*-hpux11.[01]*|*-irix*|*-osf*">v&gt;=0.9.25</depend>
    </dependencies>

    <script-header>
<![CDATA[
_datadir="${SB_INSTALL_PREFIX}/share"
]]>
    </script-header>

    <test auto-include="0">
<![CDATA[
gmake test
]]>
    </test>

    <module name="default">
      <install-name>lib${SB_PROG_NAME}10</install-name>

      <validate>
        <depend program="openssl" module="64bit"
          systype="*-aix*|*-hpux*|*-irix*|*-solaris*">v==1.0.0e</depend>
      </validate>

      <script-header>
<![CDATA[
_datadir="${SB_INSTALL_PREFIX}/share"
]]>
      </script-header>

      <configure>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix*)
  ARCH=aix-cc ;;
ia64*-hpux*)
  ARCH=hpux-ia64-cc ;;
*-hpux11.[01]*)
  ARCH=hpux-parisc-cc ;;
*-hpux*)
  ARCH=hpux-parisc2-cc ;;
*-irix*)
  ARCH=irix-mips3-cc ;;
*-freebsd*)
  ARCH=BSD-x86-elf ;;
x86_64*-linux*)
  ARCH=linux-x86_64 ;;
i?86*-linux*)
  ARCH=rhel4-elf ;;
*-linux*)
  ARCH=linux-elf ;;
*-osf*)
  ARCH=tru64-alphaev5-cc ;;
i?86*-solaris*)
  ARCH=solaris-x86-cc ;;
*-solaris2.8)
  ARCH=solaris-sparcv8-cc ;;
*-solaris*)
  ARCH=solaris-sparcultra2-cc ;;
esac

if [ "${SB_VAR_PRNGD+set}" = "set" ]; then
  PRNGD_SOCKET=/var${SB_VAR_PRNGD}/pool

  # set path to PRNGD socket
  ${SB_PATH_SED} -i -e "\
s!^is located at.*!is located at ${PRNGD_SOCKET}.!" \
  doc/crypto/RAND_egd.pod

  perl ./Configure ${ARCH} no-idea no-mdc2 no-rc5 no-ec \
  no-ecdh no-ecdsa shared -DDEVRANDOM_EGD=\"${PRNGD_SOCKET}\" \
  --openssldir=${_datadir} \
  --enginesdir="${SB_INSTALL_PREFIX_LIB}/engines" \
  --prefix=${SB_INSTALL_PREFIX}
else
  perl ./Configure ${ARCH} no-idea no-mdc2 no-rc5 no-ec \
  no-ecdh no-ecdsa shared --openssldir=${_datadir} \
  --enginesdir="${SB_INSTALL_PREFIX_LIB}/engines" \
  --prefix=${SB_INSTALL_PREFIX}
fi
]]>
      </configure>

      <build>
<![CDATA[
_libdir=${SB_INSTALL_PREFIX_LIB##${SB_INSTALL_PREFIX}}
gmake LIBDIR=${_libdir}
]]>
      </build>

      <test auto-include="0">
<![CDATA[
gmake test
]]>
      </test>

      <install>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix*)
  CC_LD_RT="-Wl,-blibpath:" ;;
ia64*-hpux*)
  CC_LD_RT="-Wl,+b," ;;
*-hpux*)
  CC_LD_RT="-Wl,+s,+b," ;;
*-freebsd*|*-irix*|*-linux*|*-osf*)
  CC_LD_RT="-Wl,-rpath," ;;
*-solaris*)
  CC_LD_RT="-R" ;;
esac

_libdir=${SB_INSTALL_PREFIX_LIB##${SB_INSTALL_PREFIX}/}
gmake install LIBDIR=${_libdir} \
MANDIR="${SB_INSTALL_PREFIX}/man"

# Embed runtime library path
case "${SB_SYSTYPE}" in
hppa*-hpux*)
  rm libssl.sl.1.0.0 libssl.sl
  gmake libssl.sl.1.0.0 LIBDIR=${_libdir} \
  LIBDEPS="-L${SB_INSTALL_PREFIX_LIB} -lcrypto -ldld"
  gmake install_lib LIBDIR=${_libdir}

  rm apps/openssl
  gmake build_apps LIBDIR=${_libdir} \
  LIBCRYPTO="-L${SB_INSTALL_PREFIX_LIB} -lcrypto" \
  LIBSSL="-L${SB_INSTALL_PREFIX_LIB} \
${CC_LD_RT}${SB_INSTALL_PREFIX_LIB} -lssl"
  gmake install_apps LIBDIR=${_libdir}

  gmake -C engines clean_libobj
  gmake build_engines LIBDIR=${_libdir} \
  LIBDEPS="-L${SB_INSTALL_PREFIX_LIB} -lcrypto"
  gmake install_engines LIBDIR=${_libdir} ;;
esac

# Fix library path in pkgconfig file
${SB_PATH_SED} -i -e "\
s!-L\(\${libdir}\)!-L\1 ${CC_LD_RT}\1!" \
${SB_INSTALL_PREFIX_LIB}/pkgconfig/libcrypto.pc \
${SB_INSTALL_PREFIX_LIB}/pkgconfig/libssl.pc \
${SB_INSTALL_PREFIX_LIB}/pkgconfig/openssl.pc

# copy certificate files
${SB_PATH_INSTALL} -m 755 -o root -g 0 -d ${_datadir}/certs
${SB_PATH_FIND} certs | egrep -v -e 'demo|expired' | cpio -pdmuv ${_datadir}
${SB_PATH_CHOWN} -hR 0:0 ${_datadir}/certs
${SB_PATH_CHMOD} 644 ${_datadir}/certs/*

# c_rehash seems to be useless
rm ${SB_INSTALL_PREFIX}/bin/c_rehash
]]>
      </install>
    </module>

