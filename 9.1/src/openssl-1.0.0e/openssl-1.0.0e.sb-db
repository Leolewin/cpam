*-solaris2.8)
  ARCH=solaris64-sparcv9-cc-old ;;
*-solaris*)
  ARCH=solaris64-sparcv9-cc ;;
esac

if [ "${SB_VAR_PRNGD+set}" = "set" ]; then
  PRNGD_SOCKET=/var${SB_VAR_PRNGD}/pool

  perl ./Configure ${ARCH} no-idea no-mdc2 no-rc5 no-ec \
  no-ecdh no-ecdsa shared --openssldir=${_datadir} \
  --enginesdir="${SB_INSTALL_PREFIX}/${_libdir}/engines" \
  --prefix=${SB_INSTALL_PREFIX}

else
  perl ./Configure ${ARCH} no-idea no-mdc2 no-rc5 no-ec \
  no-ecdh no-ecdsa shared \
  --enginesdir="${SB_INSTALL_PREFIX}/${_libdir}/engines" \
  --openssldir=${_datadir} --prefix=${SB_INSTALL_PREFIX}
fi
]]>
      </configure>

      <build>
<![CDATA[
gmake LIBDIR="${_libdir}"
]]>
      </build>

      <test auto-include="0">
<![CDATA[
gmake test
]]>
      </test>

      <install>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix*)
  CC_LD_RT="-Wl,-blibpath:" ;;
ia64*-hpux*)
  CC_LD_RT="-Wl,+b," ;;
*-hpux*)
  CC_LD_RT="-Wl,+s,+b," ;;
*-freebsd*|*-irix*|*-linux*|*-osf*)
  CC_LD_RT="-Wl,-rpath," ;;
*-solaris*)
  CC_LD_RT="-R" ;;
esac

if [ ! -d ${SB_INSTALL_PREFIX}/lib/64 ]; then
  case "${SB_SYSTYPE}" in
  *-aix*|*-hpux*|*-solaris*)
    ln -s ./$(basename ${_libdir}) ${SB_INSTALL_PREFIX}/lib/64 ;;
  *-irix*)
    ln -s ../lib64 ${SB_INSTALL_PREFIX}/lib/64 ;;
  esac
fi

gmake install_lib install_engines \
LIBDIR="${_libdir}"

# Fix library path in pkgconfig file
${SB_PATH_SED} -i -e "\
s!-L\(\${libdir}\)!-L\1 ${CC_LD_RT}\1!" \
${SB_INSTALL_PREFIX}/${_libdir}/pkgconfig/libcrypto.pc \
${SB_INSTALL_PREFIX}/${_libdir}/pkgconfig/libssl.pc \
${SB_INSTALL_PREFIX}/${_libdir}/pkgconfig/openssl.pc
]]>
      </install>
    </module>

