<?xml version="1.0"?>
<packages>
  <package name="make" version="3.82">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>GNU Make</title>
      <vendor>Free Software Foundation</vendor>

      <description attribution="The OpenPKG Project">
This is the GNU variant of make(1). The purpose of this utility is to
determine automatically which pieces of a large program need to be
recompiled, and issue the commands to recompile them.
      </description>

      <install-name>make382</install-name>

      <pkgname-base>TWWgmake382</pkgname-base>

      <version>3.82</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgettext018"
          subpkg="runtime">v&gt;=0.18.1.1</depend>
      </subpkg>

      <replace pkgname-base="TWWgmake381"
        install-name="gmake381"/>
      <replace pkgname-base="TWWgmake38"
        install-name="gmake38"/>
      <replace pkgname-base="TWWgmake"
        install-name="gmake"/>
    </package-manager>

