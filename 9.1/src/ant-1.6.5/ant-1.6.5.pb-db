<packages>
  <package name="ant" version="1.6.5" revision="5">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>Apache Ant</title>
      <vendor>Apache Software Foundation</vendor>

      <description attribution="freshmeat.net">
Ant is a Java based build tool, similar to make, but with better
support for the cross platform issues involved with developing Java
applications.
      </description>

      <install-name>ant16</install-name>

      <pkgname-base>TWWant16</pkgname-base>

      <version>1.6.5</version>
      <revision>5</revision>

      <config>etc/ant.conf</config>

      <changelog>
        <change date="2012 January 31" revision="5"
        author="Gary V. Vaughan" email="gary@thewrittenword.com">
          <item><para>Update path to Perl interpreter in
<command>contrib/*.pl</command> files from 5.8.8 to
5.12.2</para></item>
          <item><para>Update path to Python interpreter in
<command>contrib/*.py</command> files from 2.5.2 to
2.7.2</para></item>
        </change>

        <change date="2010 January 18" revision="4"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update path to Python interpreter in
<command>contrib/*.py</command> files from 2.5.1 to
2.5.2</para></item>
        </change>

        <change date="2008 January 23" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update path to Python interpreter in
<command>contrib/*.py</command> files from 2.3.5 to
2.5.1</para></item>
        </change>

        <change date="2006 March 17" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add tasks for ANTLR, BCEL, Jakarta-Regex, and
Junit</para></item>
        </change>
      </changelog>
    </package-manager>

