        link-dest="/sbin/init.d/TWWdbus12"/>

      <config>/sbin/init.d/TWWdbus12</config>

      <subpkg type="conf">
        <depend group="dbus"/>
      </subpkg>
      <subpkg type="runtime">
        <depend pkgname-base="TWWexpat20"
          subpkg="runtime">v&gt;=2.0.1</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
