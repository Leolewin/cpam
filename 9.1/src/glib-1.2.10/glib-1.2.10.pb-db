<?xml version="1.0"?>
<packages>
  <package name="glib" version="1.2.10" revision="3">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>Glib Library</title>
      <vendor>The Glib Team</vendor>

      <description>
GLib is the low-level core library that forms the basis of GTK+ and
GNOME. It provides data structure handling for C, portability
wrappers, and interfaces for such runtime functionality as an event
loop, dynamic loading, and an object system.
      </description>

      <install-name>glib12</install-name>

      <pkgname-base>TWWglib12</pkgname-base>

      <version>1.2.10</version>
      <revision>4</revision>

      <changelog>
        <change date="2006 August 29" revision="4"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Quote macros in glib.m4 to avoid warnings in newer
automake releases</para></item>
        </change>

        <change date="2003 December 16" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Force use of shl_load on HP-UX 11.x</para></item>
        </change>
      </changelog>
    </package-manager>

