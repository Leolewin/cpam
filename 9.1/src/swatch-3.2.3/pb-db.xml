<?xml version="1.0"?>
<packages>
  <package name="swatch" version="3.2.3">
    <package-manager name="depot">
      <category>Applications/System</category>
      <title>Simple file watcher (swatch)</title>
      <vendor>E. Todd Atkins</vendor>

      <description>
Swatch actively monitors messages as they are written to a log file
via the UNIX syslog utility.
      </description>

      <install-name>swatch32</install-name>

      <pkgname-base>TWWswatch32</pkgname-base>

      <version>3.2.3</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWcoreutils811"
          subpkg="runtime">v&gt;=8.11</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="packages">v&gt;=5.12.2</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="runtime">v&gt;=5.12.2.</depend>
      </subpkg>

      <replace pkgname-base="TWWswatch31"
        install-name="swatch31"/>
      <replace pkgname-base="TWWswatch30"
        install-name="swatch30"/>
    </package-manager>

