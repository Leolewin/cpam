<packages>
  <package name="glib" version="2.26.1" revision="3">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>Glib Library</title>
      <vendor>The Glib Team</vendor>

      <description>
GLib is the low-level core library that forms the basis of GTK+ and
GNOME. It provides data structure handling for C, portability
wrappers, and interfaces for such runtime functionality as an event
loop, threads, dynamic loading, and an object system.
      </description>

      <install-name>libglib226</install-name>

      <pkgname-base>TWWlibglib226</pkgname-base>

      <version>2.26.1</version>
      <revision>3</revision>

      <variable>
        <path name="PERL_5122_INCLUDE_PATHS_D"
          install-name="perl5122">etc/include.paths.d</path>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgettext018"
          subpkg="runtime">v&gt;=0.18.1.1</depend>
        <depend pkgname-base="TWWlibffi30"
          subpkg="runtime">v&gt;=3.0.10</depend>
        <depend pkgname-base="TWWlibiconv113"
          subpkg="runtime">v&gt;=1.13.1</depend>
        <depend pkgname-base="TWWlibpcre811"
          subpkg="runtime">v&gt;=8.11</depend>
        <depend pkgname-base="TWWlibz12"
          subpkg="runtime">v&gt;=1.2.5</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="runtime">v&gt;=5.12.2</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="12a7df27af37d0960587e83e4ae2c4c2"
          checksum-type="md5" size="9987"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <replace pkgname-base="TWWglib216"
        install-name="libglib216"/>
      <replace pkgname-base="TWWglib212"
        install-name="libglib212"/>
      <replace pkgname-base="TWWglib12"
        install-name="glib12"/>

      <changelog>
        <change date="2012 May 9" revision="3"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Add 64bit libraries for GCC</para></item>
        </change>

        <change date="2012 February 27" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Fix NULL dereference in pygobject's
pygobject_new_sunk function when running meld</para></item>
        </change>
      </changelog>
    </package-manager>

