      <configure>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix*)
  CC=xlc_r CFLAGS="-O2 -qro -qroconst -qmaxmem=-1 -qarch=ppc"
  CXX=xlC CXXFLAGS="-O2 -qro -qroconst -qmaxmem=-1 -qarch=ppc"
  LDFLAGS="-Wl,-brtl -Wl,-blibpath:/usr/lib"
  CC_LD_RT="-Wl,-blibpath:"

  ARGS="--enable-threads" ;;
ia64*-hpux*)
  # +O2 and a number of tests fail, so reduced to +O1
  CC=cc CFLAGS="-z +O1 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-AA -z +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CC_LD_RT="-Wl,+b,"

  ARGS="--enable-threads" ;;
*-hpux11.[01]*)
  CC=cc CFLAGS="-z +O2 +Ofltacc +Olit=all +DAportable \
+Oentrysched +Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-AA -z +O2 +Ofltacc +Olit=all +DAportable \
+Oentrysched +Odataprefetch +Onolimit"
  CC_LD_RT="-Wl,+s,+b,"

  ARGS="--enable-threads" ;;
*-hpux*)
  CC=cc CFLAGS="-z +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-AA -z +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CC_LD_RT="-Wl,+s,+b,"

  ARGS="--enable-threads" ;;
*-irix*)
  CC=cc CFLAGS="-Wl,-woff,84 -Wl,-woff,85 -woff 1009,1429 \
-use_readonly_const -G0 -rdata_shared -DEBUG:optimize_space=ON \
-O2 -OPT:Olimit=0"
  CXX=CC CXXFLAGS="-Wl,-woff,84 -Wl,-woff,85 -woff 1429 \
-use_readonly_const -G0 -rdata_shared -DEBUG:optimize_space=ON \
-ptused -FE:template_in_elf_section -O2 -OPT:Olimit=0"
  CC_LD_RT="-Wl,-rpath,"

  # configure incorrectly takes the #error zero exit status
  # to mean there is monotonic clock support on irix.
  ARGS="--enable-threads glib_cv_monotonic_clock=no" ;;
i?86*-linux*)
  CFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CXXFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--enable-threads" ;;
*-linux*)
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--enable-threads" ;;
*-osf*)
  CC=cc CFLAGS="-O2 -arch ev5 -ieee -msym -readonly_strings"
  CXX=cxx CXXFLAGS="-O2 -arch ev5 -model ansi -msym \
-readonly_strings -timplicit_local"
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--enable-threads" ;;
i?86*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=pentium4"
  CXX=CC CXXFLAGS="-xO2 -xtarget=pentium4"
  CC_LD_RT="-R"

  ARGS="--enable-threads ac_cv_func_vasprintf=no" ;;
*-solaris2.8)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 \
-xarch=v8plusa"
  CXX=CC CXXFLAGS="-xO2 -xtarget=ultra2 -xarch=v8plusa"
  CC_LD_RT="-R"

  ARGS="--enable-threads" ;;
*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 \
-m32 -xarch=sparcvis"
  CXX=CC CXXFLAGS="-xO2 -xtarget=ultra2 -m32 -xarch=sparcvis" 
  CC_LD_RT="-R"

  ARGS="--enable-threads" ;;
esac

${SHELL} ./configure CC="${CC:-gcc}" CFLAGS="${CFLAGS:--O2}" \
CXX="${CXX+${CXX}}" CXXFLAGS="${CXXFLAGS:--O2}" \
CPPFLAGS="-I${SB_VAR_GETTEXT_INC} -I${SB_VAR_LIBICONV_INC} \
-I${SB_VAR_LIBZ_INC} ${CPPFLAGS+${CPPFLAGS}}" \
LDFLAGS="-L${SB_VAR_GETTEXT_LIB} \
-L${SB_VAR_LIBICONV_LIB} -L${SB_VAR_LIBZ_LIB} \
${SB_VAR_GCC_RT+-L${SB_VAR_GCC_RT_LIB}} \
${CC_LD_RT}${SB_VAR_GETTEXT_LIB}:\
${SB_VAR_LIBICONV_LIB}:${SB_VAR_LIBZ_LIB}\
${SB_VAR_GCC_RT+:${SB_VAR_GCC_RT_LIB}} ${LDFLAGS+${LDFLAGS}}" \
PKG_CONFIG_PATH="${SB_VAR_GETTEXT_LIB}/pkgconfig:\
${SB_VAR_LIBICONV_LIB}/pkgconfig:${SB_VAR_LIBPCRE_LIB}/pkgconfig:\
${SB_VAR_LIBZ_LIB}/pkgconfig" \
--enable-static --enable-shared --datarootdir=${_datadir} \
--datadir=${_datadir} --with-html-dir=${_docdir} \
--disable-dtrace --disable-visibility --disable-fam --enable-debug=no \
--with-xdg-data-dirs-extra="${_twwdata_sysconfdir}/xdg-data-dirs.d" \
--with-libiconv=gnu --with-pcre=system \
--with-xml-catalog="${SB_VAR_XMLCATALOG}/catalog.xml" \
--disable-dependency-tracking ${ARGS+${ARGS}} \
--mandir="${SB_INSTALL_PREFIX}/man" \
--libdir=${SB_INSTALL_PREFIX_LIB} --prefix=${SB_INSTALL_PREFIX}

${SB_PATH_SED} -i -e "\
s!-L\(\${libdir}\)!-L\1 ${CC_LD_RT}\1!;" \
glib-2.0.pc gmodule-2.0.pc gmodule-export-2.0.pc \
gmodule-no-export-2.0.pc gobject-2.0.pc gthread-2.0.pc \
gio-2.0.pc gio-unix-2.0.pc

# Hack to avoid changing initializers in lots of test cases
case "${SB_SYSTYPE}" in
*-irix*)
  ${SB_PATH_SED} -i -e "\
s|^CC = .*|CC = c99|" glib/tests/Makefile gio/tests/Makefile ;;
esac
]]>
      </configure>

