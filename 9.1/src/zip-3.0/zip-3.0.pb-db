<?xml version="1.0"?>
<packages>
  <package name="zip" version="3.0">
    <package-manager name="depot">
      <category>Applications/Archiving</category>
      <title>Zip Compression Utility</title>
      <vendor>Info-Zip Group</vendor>

      <description>
Zip 3.0 is a compression and file packaging utility.  It is compatible
with PKZIP 2.04g (Phil Katz ZIP) for MSDOS systems.
      </description>

      <install-name>zip30</install-name>

      <pkgname-base>TWWzip30</pkgname-base>

      <version>3.0</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWbzip210"
          subpkg="runtime">v&gt;=1.0.6</depend>
      </subpkg>

      <replace pkgname-base="TWWzip"
        install-name="zip"/>
    </package-manager>

    <package-manager name="inst">
      <category>Applications/Archiving</category>
      <title>Zip Compression Utility</title>
      <vendor>Info-Zip Group</vendor>

      <description>
Zip 3.0 is a compression and file packaging utility.  It is compatible
with PKZIP 2.04g (Phil Katz ZIP) for MSDOS systems.
      </description>

      <install-name>zip30</install-name>

      <pkgname-base>TWWzip30</pkgname-base>

      <version>1</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWbzip210"
          subpkg="runtime">v&gt;=2</depend>
      </subpkg>

      <replace pkgname-base="TWWzip"
        install-name="zip"/>
    </package-manager>

    <package-manager name="lpp">
      <category>Applications/Archiving</category>
      <title>Zip Compression Utility</title>
      <vendor>Info-Zip Group</vendor>

      <description>
Zip 3.0 is a compression and file packaging utility.  It is compatible
with PKZIP 2.04g (Phil Katz ZIP) for MSDOS systems.
      </description>

      <install-name>zip30</install-name>

      <pkgname-base>TWW.zip30</pkgname-base>

      <version>3.0.0</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWW.bzip210"
          subpkg="runtime">v&gt;=1.0.6.1</depend>
      </subpkg>

      <replace pkgname-base="TWW.zip"
        install-name="zip"/>
    </package-manager>

    <package-manager name="pkgadd">
      <category>Applications/Archiving</category>
      <title>Zip Compression Utility</title>
      <vendor>Info-Zip Group</vendor>

      <description>
Zip 3.0 is a compression and file packaging utility.  It is compatible
with PKZIP 2.04g (Phil Katz ZIP) for MSDOS systems.
      </description>

      <install-name>zip30</install-name>

      <pkgname-base>TWWzip30</pkgname-base>

      <version>3.0</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWbzip210"
          title="Bzip2 File Compressor"
          subpkg="runtime">v&gt;=1.0.6</depend>
      </subpkg>

      <replace pkgname-base="TWWzip"
        install-name="zip"/>
    </package-manager>

    <package-manager name="rpm4">
      <category>Applications/Archiving</category>
      <title>Zip Compression Utility</title>
      <vendor>Info-Zip Group</vendor>

      <description>
Zip 3.0 is a compression and file packaging utility.  It is compatible
with PKZIP 2.04g (Phil Katz ZIP) for MSDOS systems.
      </description>

      <install-name>zip30</install-name>

      <pkgname-base>TWWzip30</pkgname-base>

      <version>3.0</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWbzip210"
          subpkg="runtime">v&gt;=1.0.6</depend>
      </subpkg>

      <replace pkgname-base="TWWzip"
        install-name="zip"/>
    </package-manager>

    <package-manager name="setld">
      <category>Applications/Archiving</category>
      <title>Zip Compression Utility</title>
      <vendor>Info-Zip Group</vendor>

      <description>
Zip 3.0 is a compression and file packaging utility.  It is compatible
with PKZIP 2.04g (Phil Katz ZIP) for MSDOS systems.
      </description>

      <install-name>zip30</install-name>

      <pkgname-base>TWWZIP30</pkgname-base>

      <version>3.0</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWBZIP210"
          subpkg="runtime">v&gt;=1.0.6</depend>
      </subpkg>

      <replace pkgname-base="TWWZIP"
        install-name="zip"/>
    </package-manager>
  </package>
</packages>
