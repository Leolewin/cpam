<?xml version="1.0"?>
<packages>
  <package name="oratcl" version="4.4" revision="2">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>Oratcl</title>
      <vendor>Oratcl developers</vendor>

      <description>
The Oratcl package is a dynamic extension of the Tcl language that
integrates Oracle OCI calls into a set of Tcl commands that allow
Oracle access via Tcl.
      </description>

      <install-name>oratcl44</install-name>

      <pkgname-base>TWWoratcl44</pkgname-base>

      <version>4.4</version>
      <revision>2</revision>

      <variable>
        <path name="TCL_85_INCLUDE_PATHS"
          install-name="tcl85">etc/include.paths</path>
        <path name="TCL_85_INCLUDE_PATHS_D"
          install-name="tcl85">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWtcl85"
          subpkg="runtime">v&gt;=8.5.10</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="0dcfb3630af82f9ad43675fbdc5cebae"
          checksum-type="md5" size="10031"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <changelog>
        <change date="2012 April 7" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update tcl dependency from 8.5.6 to 
8.5.10</para></item>
        </change>
      </changelog>
    </package-manager>

