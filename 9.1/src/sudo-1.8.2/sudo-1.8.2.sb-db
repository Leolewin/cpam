  <program name="sudo" version="1.8.2" revision="7">
    <build-name>${SB_PROG_NAME}-${SB_PROG_VER}</build-name>

    <install-name>${SB_PROG_NAME}18</install-name>

    <sources>
      <source checksum="000f458e7391be9fdf459a9ad6a4912a"
        checksum-type="md5" size="1465051"
        path="src/sudo-1.8.2.tar.gz"/>
      <patch checksum="60fa3bf4e84bcbc7edb02620747d4b20"
