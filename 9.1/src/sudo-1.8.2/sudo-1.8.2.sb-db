    <script-header>
<![CDATA[
_sysconfdir="${SB_INSTALL_PREFIX}/etc"

_docdir="${SB_INSTALL_PREFIX}/doc"
]]>
    </script-header>

    <module name="default">
      <validate>
        <depend program="sudo" module="aix-pam"
          systype="*-aix*">v==1.8.2</depend>
      </validate>

      <script-header>
<![CDATA[
_sysconfdir="${SB_INSTALL_PREFIX}/etc"

_docdir="${SB_INSTALL_PREFIX}/doc"
]]>
      </script-header>

      <configure>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix*)
  CC=xlc CFLAGS="-O2 -qro -qroconst -qmaxmem=-1 -qarch=ppc"

  ARGS="--with-authenticate" ;;
ia64*-hpux*)
  CC=cc CFLAGS="-z +O2 +Onofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  LDFLAGS="-Wl,+nodefaultrpath"

  ARGS="--with-pam" ;;
*-hpux11.[01]*)
  CC=cc CFLAGS="-z +O2 +Onofltacc +Olit=all +DAportable \
+Oentrysched +Odataprefetch +Onolimit"

  ARGS="--with-pam" ;;
*-hpux*)
  CC=cc CFLAGS="-z +O2 +Onofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"

  ARGS="--with-pam" ;;
*-irix*)
  CC=c99 CFLAGS="-Wl,-woff,84 -Wl,-woff,85 -woff 1009,1429 \
-use_readonly_const -G0 -rdata_shared -DEBUG:optimize_space=ON \
-O2 -OPT:Olimit=0"
  NROFF=awf ;;
i?86*-linux*)
  CFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"

  ARGS="--with-pam" ;;
*-linux*)
  ARGS="--with-pam" ;;
*-osf*)
  CC=cc CFLAGS="-O2 -msym -readonly_strings" ;;
i?86*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=pentium4"

  ARGS="--with-pam" ;;
*-solaris2.8)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 \
-xarch=v8plusa"

  ARGS="--with-pam" ;;
*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 \
-m32 -xarch=sparcvis"

  ARGS="--with-pam" ;;
esac

${SHELL} ./configure CFLAGS="${CFLAGS:--O2}" \
NROFFPROG=${NROFF:-nroff} --with-CC="${CC:-gcc}" \
LDFLAGS="${LDFLAGS+${LDFLAGS}}" --disable-path-info \
--enable-shell-sets-home --enable-log-host --with-logfac=auth \
--with-env-editor --with-ignore-dot --without-lecture \
--with-logging=syslog --with-goodpri=notice --with-badpri=alert \
--disable-sia --disable-root-mailer --without-blibpath \
${ARGS+${ARGS}} --docdir=${_docdir} --sysconfdir=${_sysconfdir} \
--mandir="${SB_INSTALL_PREFIX}/man" --prefix=${SB_INSTALL_PREFIX}
]]>
      </configure>

      <install>
<![CDATA[
gmake install
]]>
      </install>

      <uninstall>
<![CDATA[
rm -rf ${_sysconfdir} ${SB_INSTALL_PREFIX}
]]>
      </uninstall>
    </module>

    <module name="aix-pam"
    systype="*-aix*">
      <script-header>
<![CDATA[
_sysconfdir="${SB_INSTALL_PREFIX}/etc"

_docdir="${SB_INSTALL_PREFIX}/doc"
]]>
      </script-header>

      <configure>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix*)
  CC=xlc CFLAGS="-O2 -qro -qroconst -qmaxmem=-1 -qarch=ppc"

  ARGS="--with-pam" ;;
esac

${SHELL} ./configure CFLAGS="${CFLAGS:--O2}" \
NROFFPROG=${NROFF:-nroff} --with-CC="${CC:-gcc}" \
LDFLAGS="${LDFLAGS+${LDFLAGS}}" --disable-path-info \
--enable-shell-sets-home --enable-log-host --with-logfac=auth \
--with-env-editor --with-ignore-dot --without-lecture \
--with-logging=syslog --with-goodpri=notice --with-badpri=alert \
--disable-sia --disable-root-mailer --without-blibpath \
--sysconfdir=${_sysconfdir} ${ARGS+${ARGS}} \
--prefix=${SB_INSTALL_PREFIX}
]]>
      </configure>

      <install>
<![CDATA[
${SB_PATH_INSTALL} -o root -g 0 -d ${SB_INSTALL_PREFIX}/bin/pam
${SB_PATH_INSTALL} -o root -g 0 -m 4111 src/sudo \
${SB_INSTALL_PREFIX}/bin/pam/sudo
]]>
      </install>

      <uninstall>
<![CDATA[
rm ${SB_INSTALL_PREFIX}/bin/pam/sudo
]]>
      </uninstall>
    </module>

