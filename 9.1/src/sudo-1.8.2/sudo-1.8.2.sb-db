        path="src/sudo-1.8.2.CVE-2012-0809.patch"/>
      <patch checksum="6a4a0c6ce4841745662dfa22fb5dc2a4"
        checksum-type="md5" size="1311"
        path="src/sudo-1.8.2.CVE-2012-2337.patch"/>
      <patch checksum="72a3ccb56a1e569923314ac200bfd050"
        checksum-type="md5" size="2121"
        path="src/sudo-1.8.2.tww.patch"/>
      <patch checksum="c337f4d480224c3e5381a9abe3facbb6"
