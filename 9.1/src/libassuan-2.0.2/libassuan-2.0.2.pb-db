<?xml version="1.0"?>
<packages>
  <package name="libassuan" version="2.0.2">
    <package-manager name="depot">
      <category>Development/Libraries</category>
      <title>IPC library for GnuPG 2</title>
      <vendor>Free Software Foundation</vendor>

      <description>
IPC library used by GnuPG 2, GPGME and a few other packages.
      </description>

      <install-name>libassuan20</install-name>

      <pkgname-base>TWWlibassuan20</pkgname-base>

      <version>2.0.2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgpgerror110"
          subpkg="runtime">v&gt;=1.10</depend>
      </subpkg>

      <replace pkgname-base="TWWassuan10"
        install-name="libassuan10"/>
    </package-manager>

