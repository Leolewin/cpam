        <depend pkgname-base="TWWrcs"
          subpkg="runtime">v&gt;=1</depend>
        <depend pkgname-base="TWWsubversion16"
          subpkg="runtime">v&gt;=5</depend>
      </subpkg>

      <replace pkgname-base="TWWcvs2svn22"
        install-name="cvs2svn22"/>
      <replace pkgname-base="TWWcvs2svn20"
        install-name="cvs2svn20"/>
