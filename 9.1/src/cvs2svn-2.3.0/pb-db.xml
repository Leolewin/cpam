<?xml version="1.0"?>
<packages>
  <package name="cvs2svn" version="2.3.0">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>cvs2svn CVS-to-Subversion repository converter</title>
      <vendor>cvs2svn developers</vendor>

      <description>
cvs2svn is a script that converts a CVS repository to a Subversion
repository. It is designed for one-time conversions, not for repeated
synchronizations between CVS and Subversion.
      </description>

      <install-name>cvs2svn23</install-name>

      <pkgname-base>TWWcvs2svn23</pkgname-base>

      <version>2.3.0</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWpython26"
          subpkg="runtime">v&gt;=2.6.7</depend>
        <depend pkgname-base="TWWrcs"
          subpkg="runtime">v&gt;=5.7</depend>
        <depend pkgname-base="TWWsvn16"
          subpkg="runtime">v&gt;=1.6.17</depend>
      </subpkg>

      <replace pkgname-base="TWWcvs2svn22"
        install-name="cvs2svn22"/>
      <replace pkgname-base="TWWcvs2svn20"
        install-name="cvs2svn20"/>
    </package-manager>

