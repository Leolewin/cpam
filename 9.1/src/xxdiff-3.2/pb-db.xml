<?xml version="1.0"?>
<packages>
  <package name="xxdiff" version="3.2" revision="2">
    <package-manager name="depot">
      <category>Applications/Text</category>
      <title>Graphical file and directory comparison tool</title>
      <vendor>Martin Blais</vendor>

      <description>
xxdiff is a graphical file and directories comparison and merge tool.
It can be used for viewing the differences between two or three files,
or two directories, and can be used to produce a merged version. The
texts of the two or three files are presented side by side with their
differences highlighted with colors for easy identification.
      </description>

      <install-name>xxdiff32</install-name>

      <pkgname-base>TWWxxdiff32</pkgname-base>

      <version>3.2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWdiffutils32"
          subpkg="runtime">v&gt;=3.2</depend>
        <depend pkgname-base="TWWgcc44"
          systype="*-hpux10*|ia64*-hpux*"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWlibqt33"
          subpkg="runtime">v&gt;=3.3.8 r&gt;=3</depend>
      </subpkg>

      <replace pkgname-base="TWWxxdiff31"
        install-name="xxdiff31"/>

      <changelog>
        <change date="2012 January 16" revision="3"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gcc dependency from 4.2.4 to
4.4.6</para></item>
          <item><para>Update diffutils dependency from 3.2 to
3.2</para></item>
          <item><para>Update qt dependency revision</para></item>
        </change>
      </changelog>
    </package-manager>

