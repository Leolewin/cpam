<?xml version="1.0"?>
<packages>
  <package name="gtkmm" version="2.22.0">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>C++ interface to GTK+ GUI library</title>
      <vendor>gtkmm developers</vendor>

      <description>
gtkmm is the official C++ interface for the popular GUI library GTK+.
It closely follows GTK+ development. Highlights include typesafe
callbacks, widgets extensible via inheritance and a comprehensive set
of widgets.
      </description>

      <install-name>libgtkmm222</install-name>

      <pkgname-base>TWWlibgtkmm222</pkgname-base>

      <version>2.22.0</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          systype="hppa*-hpux*"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWlibglibmm224"
          subpkg="runtime">v&gt;=2.24.2 r&gt;=2</depend>
        <depend pkgname-base="TWWlibgtk222"
          subpkg="runtime">v&gt;=2.22.1 r&gt;=2</depend>
        <depend pkgname-base="TWWlibpangomm228"
          subpkg="runtime">v&gt;=2.28.2 r&gt;=2</depend>
      </subpkg>

      <replace pkgname-base="TWWlibgtkmm212"
        install-name="libgtkmm212"/>
      <replace pkgname-base="TWWlibgtkmm210"
        install-name="libgtkmm210"/>
      <replace pkgname-base="TWWlibgtkmm26"
        install-name="libgtkmm26"/>

      <changelog>
        <change date="2012 May 12" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add 64bit libraries for GCC</para></item>
          <item><para>Add native 64bit libraries</para></item>
        </change>
      </changelog>
    </package-manager>

