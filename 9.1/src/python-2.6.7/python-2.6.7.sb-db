crypt cryptmodule.c -lcrypt
dbm dbmmodule.c -DHAVE_NDBM_H
imageop imageop.c
nis nismodule.c
_EOT_
  ;;
*-linux*)
  cat >>Modules/Setup.local <<_EOT_
audioop audioop.c
crypt cryptmodule.c -lcrypt
dbm dbmmodule.c -DHAVE_GDBM_NDBM_H -lgdbm
imageop imageop.c
nis nismodule.c -lnsl
_EOT_
  ;;
*-osf*)
  cat >>Modules/Setup.local <<_EOT_
crypt cryptmodule.c
dbm dbmmodule.c -DHAVE_NDBM_H
nis nismodule.c
_EOT_
  ;;
*-solaris*)
  cat >>Modules/Setup.local <<_EOT_
audioop audioop.c -lm
crypt cryptmodule.c
dbm dbmmodule.c -DHAVE_NDBM_H
imageop imageop.c
nis nismodule.c -lnsl
_EOT_
  ;;
esac

# add path to 3rd-party libraries to default search path
${SB_PATH_SED} -i -e "\
s!prefixes = .*!prefixes = [sys.prefix, '${SB_VAR_PYTHONP}']!" \
Lib/site.py

# curses KEY_COMMAND and KEY_HELP don't exist on AIX
case "${SB_SYSTYPE}" in
*-aix*)
  ${SB_PATH_SED} -i -e "\
/KEY_COMMAND/d;\
/KEY_HELP/d" Lib/curses/has_key.py ;;
esac

${SHELL} ./configure CC="${CC:-gcc}" OPT="${OPT:--O2 -fwrapv}" \
CPPFLAGS="${SB_VAR_LIBZ+-I${SB_VAR_LIBZ_INC}} \
${SB_VAR_NCURSES+-I${SB_VAR_NCURSES_INC}} \
${SB_VAR_NCURSES+-I${SB_VAR_NCURSES_INC}/ncurses} \
${SB_VAR_LIBDB+-I${SB_VAR_LIBDB_INC}} \
${CPPFLAGS+${CPPFLAGS}}" CFLAGS="${CFLAGS+${CFLAGS}}" \
CXXFLAGS="${CXXFLAGS:-${CFLAGS+${CFLAGS}}}" \
LDFLAGS="${_gcc_rt_lib+-L${_gcc_rt_lib}} \
${SB_VAR_LIBZ+-L${SB_VAR_LIBZ_LIB}} \
${_gcc_rt_lib+${LD_RT}${_gcc_rt_lib}} \
${LDFLAGS+${LDFLAGS}}" ${CXX+CXX="${CXX}"} \
${CXX+--with-cxx-main=yes} --with-gcc \
${ARGS+${ARGS}} --sysconfdir=${_sysconfdir} \
--with-system-ffi --mandir="${SB_INSTALL_PREFIX}/man" \
--libdir=${SB_INSTALL_PREFIX_LIB} --prefix=${SB_INSTALL_PREFIX}

# add path to include files from packages directory
grep ${SB_INSTALL_PREFIX} Lib/distutils/sysconfig.py >/dev/null || {
  if [ "${_gcc_rt_lib+set}" = set ]; then
    ${SB_PATH_SED} -i -e "\
/ldshared = ldshared.*CPPFLAGS/ {
a\\
        opt = opt + ' -I${SB_VAR_PYTHONP}/include/python2.6'
a\\
        ldshared = ldshared + ' -L${_gcc_rt_lib}'
a\\
        ldshared = ldshared + ' ${LD_RT}${_gcc_rt_lib}${LD_RT_POST+${LD_RT_POST}}'
}" Lib/distutils/sysconfig.py
  else
    ${SB_PATH_SED} -i -e "\
/ldshared = ldshared.*CPPFLAGS/ {
a\\
        opt = opt + ' -I${SB_VAR_PYTHONP}/include/python2.6'
}" Lib/distutils/sysconfig.py
  fi
}

if [ "${_gcc_rt_lib+set}" = set ]; then
  ${SB_PATH_SED} -i -e "\
s!^BLDSHARED\(.*\)!BLDSHARED\1 -L${_gcc_rt_lib} \
${LD_RT}${_gcc_rt_lib}${LD_RT_POST+${LD_RT_POST}}!" \
  Makefile
fi
]]>
    </configure>

    <build>
<![CDATA[
rm -rf Modules/expat
# osf tries to link with -pthread -lpthread which leads to a segv
# so we override the 'LIBS=-lrt -lpthread' setting in Makefile:
case "${SB_SYSTYPE}" in
*-osf*)
  LIBS=-lrt ;;
esac

# make archive members shared so we can create a shared libpython2.6
gmake CFLAGS='$(OPT) -fPIC'${LIBS+ LIBS="${LIBS}"}${LDLAST+ LDLAST="${LDLAST}"}

case "${SB_SYSTYPE}" in
*-aix*)
  ld -r -o libpython2.6.o -bnogc libpython2.6.a
  rm -f libpython2.6.exp
  nm -BCpg libpython2.6.o | awk '
{
  if ((($2 == "T") || ($2 == "D") || ($2 == "B")) && (substr($3,1,1) != ".")) {
    print $3
  }
}' | sort -u >libpython2.6.exp
  ld -o libpython2.6.so libpython2.6.o -lc -bnoentry -brtl \
  -bexport:libpython2.6.exp -G
  rm libpython2.6.o ;;
ia64*-hpux*)
  ld +vnocompatwarnings -b -o libpython2.6.so \
  +h libpython2.6.so -Fl libpython2.6.a -lpthread -lrt -lm -lc ;;
*-hpux*)
  ld +vnocompatwarnings -b +s -o libpython2.6.sl \
  +h libpython2.6.sl -Fl libpython2.6.a -lpthread -lrt -lm -lc ;;
*-irix*)
  ld -shared -o libpython2.6.so -soname libpython2.6.so \
  -all libpython2.6.a -lpthread -lm -lc ;;
*-freebsd*)
  ld -shared -o libpython2.6.so -soname=libpython2.6.so \
  -Bsymbolic --whole-archive libpython2.6.a --no-whole-archive \
  -lpthread -lutil -lm -lc ;;
*-linux*)
  ld -shared -o libpython2.6.so -soname=libpython2.6.so \
  -Bsymbolic --whole-archive libpython2.6.a --no-whole-archive \
  -lpthread -ldl -lutil -lm -lc ;;
*-osf*)
  ld -shared -msym -o libpython2.6.so -soname libpython2.6.so \
  -set_version 2.6 -all libpython2.6.a -none -lpthread -lrt -lm -lc ;;
*-solaris2.10)
  ld -G -h libpython2.6.so -z text -o libpython2.6.so \
  -z allextract libpython2.6.a -z defaultextract \
  -lresolv -lsocket -lnsl -lrt -lm -lc ;;
*-solaris*)
  ld -G -h libpython2.6.so -z text -o libpython2.6.so \
  -z allextract libpython2.6.a -z defaultextract \
  -lresolv -lsocket -lnsl -lrt -ldl -lm -lc ;;
esac
]]>
    </build>

