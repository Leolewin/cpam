    <script-header>
<![CDATA[
[ "${SB_VAR_GCC_RT+set}" = set ] && _gcc_rt_lib=${SB_VAR_GCC_RT_LIB}

_docdir="${SB_INSTALL_PREFIX}/doc"
_sysconfdir="${SB_INSTALL_PREFIX}/etc"

_sitedir=${SB_VAR_PYTHONP}

case "${SB_SYSTYPE}" in
*-aix*)
  CXX=g++ OPT="-O2 -fwrapv -mcpu=powerpc"
  LD_RT="-Wl,-brtl -Wl,-blibpath:" LD_RT_POST=":/usr/lib"
  CPPFLAGS="-D_LARGE_FILES=1 -D_THREAD_SAFE"
  LDFLAGS="-Wl,-brtl -Wl,-blibpath:/usr/lib"
  BYTEORDER="4321"
  ARGS="--with-threads --disable-ipv6"

  # For some reason, GCC accepts -pthread which is wrong
  export ac_cv_thread=no ;;
ia64*-hpux*)
  CXX=g++ CXXFLAGS="-D__STDC_EXT__"
  LD_RT="-Wl,+b,"
  CPPFLAGS="-D_FILE_OFFSET_BITS=64"
  BYTEORDER="4321"
  ARGS="--with-threads"

  export ac_cv_thread=no ;;
*-hpux11.[01]*)
  CXX=g++ CXXFLAGS="-D__STDC_EXT__"
  OPT="-O2 -fwrapv -march=1.1"
  LD_RT="-Wl,+s,+b,"
  CPPFLAGS="-D_FILE_OFFSET_BITS=64"
  BYTEORDER="4321"

  ARGS="--with-threads --disable-ipv6"

  # For some reason, GCC accepts -pthread which is wrong
  export ac_cv_thread=no

  # getnameinfo requires newer libc
  export ac_cv_func_getnameinfo=no ;;
*-hpux*)
  CXX=g++ CXXFLAGS="-D__STDC_EXT__"
  LD_RT="-Wl,+s,+b,"
  CPPFLAGS="-D_FILE_OFFSET_BITS=64"
  BYTEORDER="4321"

  ARGS="--with-threads"

  # HPPA/HPUX 11.31 enables posix semaphores, but builds a module with
  # undefined symbols that can not be loaded
  export ac_cv_posix_semaphores_enabled=no

  # For some reason, GCC accepts -pthread which is wrong
  export ac_cv_thread=no ;;
*-irix*)
  CXX=g++
  LD_RT="-Wl,-rpath,"
  ARGS="--with-threads"
  BYTEORDER="4321"

  # disable for compatibility with older OS releases
  export ac_cv_func_getaddrinfo=no
  export ac_cv_func_getnameinfo=no ;;
*-freebsd*)
  CXX=g++ OPT="-O2 -fwrapv -mtune=pentium4 -march=pentium4"
  LD_RT="-Wl,-rpath,"
  BYTEORDER="1234"

  ARGS="--with-threads" ;;
x86_64*-linux*)
  CXX=g++
  LD_RT="-Wl,-rpath,"
  BYTEORDER="1234"


  ARGS="--with-threads" ;;
i?86*-linux*)
  OPT="-O2 -fwrapv -mtune=pentiumpro -march=pentiumpro"
  LD_RT="-Wl,-rpath,"
  BYTEORDER="1234"

  ARGS="--with-threads" ;;
*-linux*)
  LD_RT="-Wl,-rpath,"
  BYTEORDER="1234"

  ARGS="--with-threads" ;;
*-osf*)
  CXX=g++ OPT="-O2 -fwrapv -mcpu=ev5 -mieee"
  LD_RT="-Wl,-rpath,"
  CPPFLAGS="-D_REENTRANT"
  BYTEORDER="1234"

  _ssl_client_ldflags="-Wl,-B,symbolic"
  ARGS="--with-dec-threads" ;;
i?86*-solaris*)
  CXX=g++ CPPFLAGS="-D_REENTRANT -D_FILE_OFFSET_BITS=64"
  OPT="-O2 -fwrapv -mtune=pentium4 -march=pentium4"
  LD_RT="-Wl,-R,"
  BYTEORDER="1234"

  ARGS="--with-threads" ;;
*-solaris*)
  CXX=g++ CPPFLAGS="-D_REENTRANT -D_FILE_OFFSET_BITS=64"
  OPT="-O2 -fwrapv -mcpu=ultrasparc -mvis"
  LD_RT="-Wl,-R,"
  BYTEORDER="4321"

  ARGS="--with-threads" ;;
esac
]]>
    </script-header>

    <configure>
<![CDATA[
case "${SB_SYSTYPE}" in
x86_64*-linux*)
  X_LIB="-L/usr/X11R6/lib64 -Wl,-rpath,/usr/X11R6/lib64" ;;
*-linux*)
  X_LIB="-L/usr/X11R6/lib -Wl,-rpath,/usr/X11R6/lib" ;;
*-solaris*)
  X_INC="-I/usr/openwin/include"
  X_LIB="-L/usr/openwin/lib -R/usr/openwin/lib" ;;
esac

if [ "${SB_VAR_LIBFFI+set}" = set ]; then
  ${SB_PATH_SED} -i -e "\
s|ffi_inc =.*|ffi_inc = [\"${SB_VAR_LIBFFI_INC}\"]|;\
s|ffi_libdir =.*|ffi_libdir = \"${SB_VAR_LIBFFI_LIB}\"|;" \
  setup.py
fi

cat >Modules/Setup.local <<_EOT_
resource resource.c
syslog syslogmodule.c
termios termios.c
timing timingmodule.c

*shared*
_ssl _ssl.c -DUSE_SSL ${SB_VAR_LIBOPENSSL+-I${SB_VAR_LIBOPENSSL_INC}} \
 $(test "${SB_VAR_LIBOPENSSL+set}" != set && echo -I/usr/kerberos/include) \
 ${SB_VAR_LIBOPENSSL+-L${SB_VAR_LIBOPENSSL_LIB}} \
 ${_ssl_client_ldflags+${_ssl_client_ldflags}} \
 ${SB_VAR_LIBOPENSSL+${LD_RT}${SB_VAR_LIBOPENSSL_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lssl -lcrypto
_hashlib _hashopenssl.c ${SB_VAR_LIBOPENSSL+-I${SB_VAR_LIBOPENSSL_INC}} \
 ${SB_VAR_LIBOPENSSL+-L${SB_VAR_LIBOPENSSL_LIB}} \
 ${_ssl_client_ldflags+${_ssl_client_ldflags}} \
 ${SB_VAR_LIBOPENSSL+${LD_RT}${SB_VAR_LIBOPENSSL_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lssl -lcrypto
_bsddb _bsddb.c ${SB_VAR_LIBDB+-I${SB_VAR_LIBDB_INC}} \
 ${SB_VAR_LIBDB+-L${SB_VAR_LIBDB_LIB}} \
 ${SB_VAR_LIBDB+${LD_RT}${SB_VAR_LIBDB_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -ldb
_curses _cursesmodule.c ${SB_VAR_NCURSES+-I${SB_VAR_NCURSES_INC}} \
 ${SB_VAR_NCURSES+-I${SB_VAR_NCURSES_INC}/ncurses} \
 ${SB_VAR_NCURSES+-L${SB_VAR_NCURSES_LIB}} \
 ${SB_VAR_NCURSES+${LD_RT}${SB_VAR_NCURSES_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lncurses
_curses_panel _curses_panel.c ${SB_VAR_NCURSES+-I${SB_VAR_NCURSES_INC}} \
 ${SB_VAR_NCURSES+-I${SB_VAR_NCURSES_INC}/ncurses} \
 ${SB_VAR_NCURSES+-L${SB_VAR_NCURSES_LIB}} \
 ${SB_VAR_NCURSES+${LD_RT}${SB_VAR_NCURSES_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lpanel -lncurses
_tkinter _tkinter.c tkappinit.c -DWITH_APPINIT \
 -I${SB_VAR_TCL_INC} -I${SB_VAR_TK_INC} ${X_INC+${X_INC}} \
 -L${SB_VAR_TCL_LIB} -L${SB_VAR_TK_LIB} \
 ${LD_RT}${SB_VAR_TCL_LIB}:${SB_VAR_TK_LIB}${LD_RT_POST+${LD_RT_POST}} \
 -ltcl8.5 -ltk8.5 ${X_LIB+${X_LIB}} -lX11
bz2 bz2module.c ${SB_VAR_BZIP2+-I${SB_VAR_BZIP2_INC}} \
 ${SB_VAR_BZIP2+-L${SB_VAR_BZIP2_LIB}} \
 ${SB_VAR_BZIP2+${LD_RT}${SB_VAR_BZIP2_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lbz2
gdbm gdbmmodule.c ${SB_VAR_LIBGDBM+-I${SB_VAR_LIBGDBM_INC}} \
 ${SB_VAR_LIBGDBM+-L${SB_VAR_LIBGDBM_LIB}} \
 ${SB_VAR_LIBGDBM+${LD_RT}${SB_VAR_LIBGDBM_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lgdbm
pyexpat pyexpat.c -DHAVE_EXPAT_CONFIG_H \
 ${SB_VAR_LIBEXPAT+-I${SB_VAR_LIBEXPAT_INC}} \
 ${SB_VAR_LIBEXPAT+-L${SB_VAR_LIBEXPAT_LIB}} \
 ${SB_VAR_LIBEXPAT+${LD_RT}${SB_VAR_LIBEXPAT_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lexpat
_elementtree _elementtree.c -DUSE_PYEXPAT_CAPI \
 ${SB_VAR_LIBEXPAT+-I${SB_VAR_LIBEXPAT_INC}}
readline readline.c -DHAVE_RL_PRE_INPUT_HOOK -DHAVE_RL_CALLBACK \
 -DHAVE_RL_COMPLETION_APPEND_CHARACTER -DHAVE_RL_COMPLETION_MATCHES \
 ${SB_VAR_READLINE+-I${SB_VAR_READLINE_INC}} \
 ${SB_VAR_READLINE+-L${SB_VAR_READLINE_LIB}} \
 ${SB_VAR_READLINE+${LD_RT}${SB_VAR_READLINE_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lreadline
# Workaround for not being able to have an equals sign in the module
# line
SQLITE_MOD_DEFINE='-DMODULE_NAME="sqlite3"'
_sqlite3 _sqlite/cache.c _sqlite/connection.c _sqlite/cursor.c \
 _sqlite/microprotocols.c _sqlite/module.c _sqlite/prepare_protocol.c \
 _sqlite/row.c _sqlite/statement.c _sqlite/util.c \
 \$(SQLITE_MOD_DEFINE) -I\$(srcdir)/Modules/_sqlite \
 ${SB_VAR_LIBSQLITE+-I${SB_VAR_LIBSQLITE_INC}} \
 ${SB_VAR_LIBSQLITE+-L${SB_VAR_LIBSQLITE_LIB}} \
 ${SB_VAR_LIBSQLITE+${LD_RT}${SB_VAR_LIBSQLITE_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lsqlite3
zlib zlibmodule.c ${SB_VAR_LIBZ+-I${SB_VAR_LIBZ_INC}} \
 ${SB_VAR_LIBZ+-L${SB_VAR_LIBZ_LIB}} \
 ${SB_VAR_LIBZ+${LD_RT}${SB_VAR_LIBZ_LIB}}\
${LD_RT_POST+${LD_RT_POST}} -lz
_EOT_

case "${SB_SYSTYPE}" in
*-aix*)
  cat >>Modules/Setup.local <<_EOT_
audioop audioop.c
crypt cryptmodule.c
dbm dbmmodule.c -DHAVE_NDBM_H
imageop imageop.c
nis nismodule.c
_EOT_
  ;;
*-hpux*)
  cat >>Modules/Setup.local <<_EOT_
audioop audioop.c
crypt cryptmodule.c
dbm dbmmodule.c -DHAVE_NDBM_H
imageop imageop.c
nis nismodule.c -lnsl
_EOT_
  ;;
*-irix*)
  cat >>Modules/Setup.local <<_EOT_
audioop audioop.c
crypt cryptmodule.c
dbm dbmmodule.c -DHAVE_NDBM_H
imageop imageop.c
nis nismodule.c
sgi sgimodule.c
_EOT_
  ;;
*-freebsd*)
  cat >>Modules/Setup.local <<_EOT_
