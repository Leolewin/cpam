    <package-manager name="depot">
      <category>Development/Languages</category>
      <title>Python Interpreter</title>
      <vendor>Guido van Rossum</vendor>
      <vendor subpkg="packages">3rd Parties</vendor>

      <description attribution="The OpenPKG Project">
Python is an interpreted, interactive, object-oriented programming
language. Python combines remarkable power with very clear syntax.  It
has modules, classes, exceptions, very high level dynamic data types,
and dynamic typing. There are interfaces to many system calls and
libraries, as well as to various windowing systems (X11, Motif, Tk,
Mac, MFC). New built-in modules are easily written in C or C++.
Python is also usable as an extension language for applications that
need a programmable interface. It is also usable as an extension
language for applications that need a programmable interface.
      </description>

      <install-name>python26</install-name>

      <pkgname-base>TWWpython26</pkgname-base>

      <version>2.6.7</version>
      <revision>1</revision>

      <variable>
        <path name="TCL_PATH"
          install-name="tcl85"/>
        <path name="TKP_PATH"
          install-name="tk85p"/>
        <path name="TK_PATH"
          install-name="tk85"/>
        <path name="XDGDATA_DATADIR_PATH"
          install-name="xdgdata12">share</path>
      </variable>

      <wrapper name="python" path="pkg/python-wrapper.c"
        checksum="5ad3eca8bdb5b112d00a972057897204"
        checksum-type="md5" size="2072"/>
      <wrapper name="python2.6" path="pkg/python-wrapper.c"
        checksum="5ad3eca8bdb5b112d00a972057897204"
        checksum-type="md5" size="2072"/>

      <config>etc/include.paths</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWlibopenssl10"
          subpkg="runtime">v&gt;=1.0.0e</depend>
      </subpkg>

      <replace pkgname-base="TWWpython25"
        install-name="python25"/>
      <replace pkgname-base="TWWpy242"
        install-name="python242"/>
    </package-manager>

