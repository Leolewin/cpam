    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>XSLT C Library</title>
      <vendor>Daniel Veillard</vendor>

      <description attribution="The OpenPKG Project">
LibXSLT is a XML Stylesheet Language (XSL) Transformation C library
developed for the Gnome project. It provides an XSLT engine for
transforming XML documents into other (output) markup languages.
      </description>

      <install-name>libxslt11</install-name>

      <pkgname-base>TWWlibxsl11</pkgname-base>

      <version>1.1.26</version>
      <revision>1</revision>

      <variable>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgcrypt14"
          subpkg="runtime">v&gt;=1.4.6</depend>
        <depend pkgname-base="TWWlibxml27"
          subpkg="runtime">v&gt;=2.7.8</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
