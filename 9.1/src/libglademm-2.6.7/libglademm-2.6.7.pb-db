<?xml version="1.0"?>
<packages>
  <package name="libglademm" version="2.6.7" revision="2">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>C++ interface to Glade library</title>
      <vendor>libglademm developers</vendor>

      <description>
libglademm is the C++ interface for the Glade library.
      </description>

      <install-name>libglademm26</install-name>

      <pkgname-base>TWWlibglademm26</pkgname-base>

      <version>2.6.7</version>
      <revision>3</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWlglade26"
          subpkg="runtime">v&gt;=2.6.4 r&gt;=2</depend>
        <depend pkgname-base="TWWlibgtkmm222"
          subpkg="runtime">v&gt;=2.22.0</depend>
      </subpkg>

      <changelog>
        <change date="2012 June 20" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add 64bit libraries for GCC</para></item>
          <item><para>Add native 64bit libraries</para></item>
        </change>

        <change date="2012 February 22" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gcc dependency from 4.2.4 to
4.4.6</para></item>
          <item><para>Update libglade dependency from 2.6.3 to
2.6.4</para></item>
          <item><para>Update gtkmm dependency from 2.12.8 to
2.22.0</para></item>
        </change>
      </changelog>
    </package-manager>

