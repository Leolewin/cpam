<packages>
  <package name="xmlto" version="0.0.24">
    <package-manager name="depot">
      <category>Applications/Text</category>
      <title>Xmlto - apply XSL stylesheet to XML document</title>
      <vendor>Tim Waugh</vendor>

      <description>
Xmlto is a front-end to an XSL toolchain. It chooses an appropriate
stylesheet for the conversion you want and applies it using an
external XSL-T processor. It also performs any necessary
post-processing.
      </description>

      <install-name>xmlto00</install-name>

      <pkgname-base>TWWxmlto00</pkgname-base>

      <version>0.0.24</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWbash42"
          subpkg="runtime">v&gt;=4.2</depend>
        <depend pkgname-base="TWWcoreutils811"
          subpkg="runtime">v&gt;=8.11</depend>
        <depend pkgname-base="TWWfind44"
          subpkg="runtime">v&gt;=4.4.2</depend>
        <depend pkgname-base="TWWgetopt11"
          subpkg="runtime">v&gt;=1.1.4</depend>
        <depend pkgname-base="TWWlibxml27"
          subpkg="runtime">v&gt;=2.7.8</depend>
        <depend pkgname-base="TWWlibxsl11"
          subpkg="runtime">v&gt;=1.1.26</depend>
        <depend pkgname-base="TWWlynx28"
          subpkg="runtime">v&gt;=2.8.7</depend>
        <depend pkgname-base="TWWmktemp17"
          subpkg="runtime">v&gt;=1.7</depend>
        <depend pkgname-base="TWWzip30"
          subpkg="runtime">v&gt;=3.0</depend>
      </subpkg>
    </package-manager>

