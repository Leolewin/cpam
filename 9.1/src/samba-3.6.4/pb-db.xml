        link-dest="/sbin/init.d/TWWsamba364"/>

      <config>/sbin/init.d/TWWsamba364</config>
      <config upgrade="1">etc/lmhosts</config>
      <config upgrade="1">etc/smb.conf</config>

      <subpkg type="runtime">
