        path="src/cppunit-1.12.1.tar.gz"/>
      <patch checksum="cdbbf9e309ef7f35b4dde9a8d9c88997"
        checksum-type="md5" size="3595"
        path="src/cppunit-1.12.1.tww.patch"/>
      <patch checksum="1fe9fc6122f4fdde4f198e4a34c38684"
