<?xml version="1.0"?>
<packages>
  <package name="lxml" version="2.2.8">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>lxml - Pythonic binding for libxml2, libxslt</title>
      <vendor>Stefan Behnel</vendor>

      <description>
lxml is a Pythonic binding for the libxml2 and libxslt libraries. It
is unique in that it combines the speed and feature completeness of
these libraries with the simplicity of a native Python API, mostly
compatible but superior to the ElementTree API.
      </description>

      <install-name>lxml22</install-name>

      <pkgname-base>TWWlxml22</pkgname-base>

      <version>2.2.8</version>
      <revision>1</revision>

      <variable>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibxml27"
          subpkg="runtime">v&gt;=2.7.8</depend>
        <depend pkgname-base="TWWpython26"
          subpkg="runtime">v&gt;=2.6.7</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="fafedd34a2439721dea56d00f3ab16fc"
          checksum-type="md5" size="9987"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <replace pkgname-base="TWWlxml13"
        install-name="lxml13"/>
      <replace pkgname-base="TWWlxml10"
        install-name="lxml10"/>
    </package-manager>

