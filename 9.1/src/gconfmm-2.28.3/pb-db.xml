<?xml version="1.0"?>
<packages>
  <package name="gconfmm" version="2.28.3">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>C++ interface to GConf library</title>
      <vendor>gconfmm developers</vendor>

      <description>
C++ wrappers for GConf.
      </description>

      <install-name>libgconfmm228</install-name>

      <pkgname-base>TWWlibgconfmm228</pkgname-base>

      <version>2.28.3</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWgnome232"
          subpkg="runtime">v&gt;=2.32</depend>
        <depend pkgname-base="TWWlibgtkmm222"
          subpkg="runtime">v&gt;=2.22.0</depend>
      </subpkg>

      <replace pkgname-base="TWWlibgconfmm222"
        install-name="libgconfmm222"/>
      <replace pkgname-base="TWWlibgconfmm218"
        install-name="libgconfmm218"/>
      <replace pkgname-base="TWWlibgconfmm210"
        install-name="libgconfmm210"/>
    </package-manager>

