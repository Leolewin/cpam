    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>GD Graphics Library</title>
      <vendor>Thomas Boutell</vendor>

      <description>
GD is a graphics library for creating PNG, JPEG, and WBMP images. It
allows your code to quickly draw images complete with lines, arcs,
text, multiple colors, cut and paste from other images, and flood
fills.
      </description>

      <install-name>libgd20</install-name>

      <pkgname-base>TWWlibgd20</pkgname-base>

      <version>2.0.35</version>
      <revision>4</revision>

      <variable>
        <path name="PERL_5122_INCLUDE_PATHS_D"
          install-name="perl5122">etc/include.paths.d</path>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWfcpackage28"
          subpkg="runtime">v&gt;=2.8</depend>
        <depend pkgname-base="TWWlibjpeg8"
          subpkg="runtime">v&gt;=8b</depend>
        <depend pkgname-base="TWWlibpng14"
          subpkg="runtime">v&gt;=1.4.4</depend>
        <depend pkgname-base="TWWlibttf24"
          subpkg="runtime">v&gt;=2.4.6</depend>
        <depend pkgname-base="TWWlibz12"
          subpkg="runtime">v&gt;=1.2.5</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="runtime">v&gt;=5.12.2</depend>
        <depend pkgname-base="TWWxpm"
          subpkg="runtime">v&gt;=3.4k</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="a259b65bf736688a725b907fc5b0b424"
          checksum-type="md5" size="9943"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <replace pkgname-base="TWWlibgd18"
        install-name="libgd18"/>

      <changelog>
        <change date="2010 December 20" revision="4"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update fcpackage dependency from 2.6 to
2.8</para></item>
          <item><para>Update libpng dependency from 1.2.29 to
1.4.4</para></item>
          <item><para>Update freetype dependency from 2.3.7 to
2.4.6</para></item>
          <item><para>Update zlib dependency from 1.2.3 to
