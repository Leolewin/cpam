<?xml version="1.0"?>
<packages>
  <package name="gnutls" version="2.10.5">
    <package-manager name="depot">
      <category>Development/Libraries</category>
      <title>GNU TLS/SSL library (gnutls)</title>
      <vendor>Free Software Foundation</vendor>

      <description>
This is a TLS (Transport Layer Security) 1.0, 1.1 and SSL (Secure
Sockets Layer) 3.0 library for the GNU project.
      </description>

      <install-name>libgnutls210</install-name>

      <pkgname-base>TWWlibgnutls210</pkgname-base>

      <version>2.10.5</version>
      <revision>1</revision>

      <variable>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgcrypt14"
          subpkg="runtime">v&gt;=1.4.6</depend>
        <depend pkgname-base="TWWlibtasn29"
          subpkg="runtime">v&gt;=2.9</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="f7f27a485c529df0a0d9fbf2d9cb3c74"
          checksum-type="md5" size="10031"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <replace pkgname-base="TWWgnutls24"
        install-name="libgnutls24"/>
    </package-manager>

