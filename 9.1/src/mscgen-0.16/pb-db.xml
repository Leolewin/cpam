<packages>
  <package name="mscgen" version="0.16">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>Message Sequence Chart parser - mscgen</title>
      <vendor>Michael McTernan</vendor>

      <description>
Mscgen parses Message Sequence Chart descriptions and produces PNG,
EPS or server side image maps (ismaps) as the output.
      </description>

      <install-name>mscgen016</install-name>

      <pkgname-base>TWWmscgen016</pkgname-base>

      <version>0.16</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgd20"
          subpkg="runtime">v&gt;=2.0.35</depend>
      </subpkg>

      <replace pkgname-base="TWWmscgen011"
        install-name="mscgen011"/>
      <replace pkgname-base="TWWmscgen07"
        install-name="mscgen07"/>
    </package-manager>

