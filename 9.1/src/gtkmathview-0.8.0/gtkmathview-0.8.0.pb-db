    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>GTK Widget to render MathML Documents - gtkmathview</title>
      <vendor>Luca Padovani</vendor>

      <description>
GtkMathView is a C++ rendering engine for MathML documents. It
provides an interactive view that can be used for browsing and editing
MathML markup.
      </description>

      <install-name>libgtkmathview08</install-name>

      <pkgname-base>TWWlibgtkmathview08</pkgname-base>

      <version>0.8.0</version>
      <revision>3</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibgtk222"
          subpkg="runtime">v&gt;=2.22.1</depend>
        <depend pkgname-base="TWWlibpopt116"
          subpkg="runtime">v&gt;=1.16</depend>
        <depend pkgname-base="TWWlibxml27"
          subpkg="runtime">v&gt;=2.7.8</depend>
      </subpkg>

      <changelog>
