<?xml version="1.0"?>
<packages>
  <package name="gwenview" version="1.4.2" revision="2">
    <package-manager name="depot">
      <category>Applications/Multimedia</category>
      <title>Gwenview KDE-based Image Viewer</title>
      <vendor>Aurelien Gateau</vendor>

      <description>
Gwenview is a simple image viewer for KDE. It features a folder tree
window and a file list window to provide easy navigation in your file
hierarchy. Image loading is done by Qt library, so it supports all
image formats your Qt installation supports.
      </description>

      <install-name>gwenview14</install-name>

      <pkgname-base>TWWgwenview14</pkgname-base>

      <version>1.4.2</version>
      <revision>2</revision>

      <variable>
        <path name="KDE_GLOBALS_PATH"
          install-name="kde35">share/config/kdeglobals</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWkde35"
          subpkg="runtime">v&gt;=3.5.10</depend>
        <depend pkgname-base="TWWlibexiv201"
          subpkg="runtime">v&gt;=0.16</depend>

        <postinstall path="pkg/add-to-kde-prefix-hpux"
          checksum="af99bcc16ae119a5afd2056b04b5f9be"
          checksum-type="md5" size="4687"/>
        <preremove path="pkg/remove-from-kde-prefix-hpux"
          checksum="208ea8d9aca0cc7a073e37acf8a01716"
          checksum-type="md5" size="1957"/>
      </subpkg>

      <replace pkgname-base="TWWgwenview13"
        install-name="gwenview13"/>

      <changelog>
        <change date="2010 January 18" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update kde dependency from 3.5.8 to
3.5.10</para></item>
        </change>
      </changelog>
    </package-manager>

