<?xml version="1.0"?>
<packages>
  <package name="grep" version="2.9">
    <package-manager name="depot">
      <category>Applications/Text</category>
      <title>GNU Grep</title>
      <vendor>Free Software Foundation</vendor>

      <description attribution="The OpenPKG Project">
Grep searches the named input FILEs (or standard input if no files are
named, or the file name - is given) for lines containing a match to
the given PATTERN. By default, grep prints the matching lines.
      </description>

      <install-name>grep29</install-name>

      <pkgname-base>TWWgrep29</pkgname-base>

      <version>2.9</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgettext018"
          subpkg="runtime">v&gt;=0.18.1.1</depend>
        <depend pkgname-base="TWWlibpcre811"
          subpkg="runtime">v&gt;=8.11</depend>
      </subpkg>

      <replace pkgname-base="TWWgrep25"
        install-name="grep25"/>
      <replace pkgname-base="TWWgrep"
        install-name="grep"/>
    </package-manager>

