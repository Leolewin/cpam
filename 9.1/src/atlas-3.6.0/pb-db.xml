<?xml version="1.0"?>
<packages>
  <package name="atlas" version="3.6.0" revision="6">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>ATLAS Linear Algebra Software</title>
      <vendor>ATLAS Team</vendor>

      <description>
The ATLAS (Automatically Tuned Linear Algebra Software) project is an
ongoing research effort focusing on applying empirical techniques in
order to provide portable performance. At present, it provides C and
Fortran77 interfaces to a portably efficient BLAS implementation, as
well as a few routines from LAPACK.
      </description>

      <install-name>libatlas36</install-name>

      <pkgname-base>TWWlatlas36</pkgname-base>

      <version>3.6.0</version>
      <revision>4</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          subpkg="libruntime">v&gt;=4.4.6</depend>
      </subpkg>

      <replace pkgname-base="TWWlatlas34"
        install-name="libatlas34"/>

      <changelog>
        <change date="2012 February 29" revision="4"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add libraries built against gcc
4.4.6</para></item>
          <item><para>Remove libraries built against gcc
4.2.4</para></item>
        </change>

        <change date="2009 August 15" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add libraries built against gcc
4.2.4</para></item>
          <item><para>Remove libraries built against gcc
3.4.3</para></item>
        </change>

        <change date="2005 June 14" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Create shared libraries in addition to static
libraries and libraries for vendor Fortran compiler and separate
libraries usable by GCC 3.4.3</para></item>
        </change>
      </changelog>
    </package-manager>

