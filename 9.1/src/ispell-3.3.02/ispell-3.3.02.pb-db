<?xml version="1.0"?>
<packages>
  <package name="ispell" version="3.3.02">
    <package-manager name="depot">
      <category>Applications/Text</category>
      <title>Ispell Spell Checker</title>
      <vendor>Geoff Kuenning</vendor>

      <description>
Ispell is a fast screen-oriented spelling checker that shows you your
errors in the context of the original file, and suggests possible
corrections when it can figure them out. Compared to UNIX spell, it is
faster and much easier to use. Ispell can also handle languages other
than English.
      </description>

      <install-name>ispell33</install-name>

      <pkgname-base>TWWispell33</pkgname-base>

      <version>3.3.02</version>
      <revision>1</revision>

      <replace pkgname-base="TWWispell32"
        install-name="ispell32"/>
    </package-manager>

