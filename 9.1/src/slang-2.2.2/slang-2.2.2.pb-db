<?xml version="1.0"?>
<packages>
  <package name="slang" version="2.2.2" revision="2">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>S-Lang Interpreter</title>
      <vendor>John E. Davis</vendor>

      <description>
S-Lang is a multi-platform programmer's library designed to allow a
developer to create robust multi-platform software. It provides
facilities required by interactive applications such as display/screen
management, keyboard input, keymaps, and so on. The most exciting
feature of the library is the slang interpreter that may be easily
embedded into a program to make it extensible.
      </description>

      <install-name>slang22</install-name>

      <pkgname-base>TWWslang22</pkgname-base>

      <version>2.2.2</version>
      <revision>2</revision>

      <replace pkgname-base="TWWslang21"
        install-name="slang21"/>
      <replace pkgname-base="TWWslang"
        install-name="slang"/>

      <changelog>
        <change date="2012 January 23" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Rebuilt package modules for new 
dependencies</para></item>
        </change>
      </changelog>
    </package-manager>

