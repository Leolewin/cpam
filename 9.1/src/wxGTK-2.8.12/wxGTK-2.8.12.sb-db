  ;;
ia64-*hpux11*)
  LDFLAGS="-Wl,+nodefaultrpath"
  CC_LD_RT="-Wl,+b," ;;
*-hpux*)
  CFLAGS="-O2"

  # HPPA gcc is inlining wxColour::IsOk and inserting an undefined
  # reference to a data symbol _ZNK8wxColour4IsOkEv, which doesn't work
  # at runtime because only the code symbol is available. At least I
  # *think* that's what's going on. Running the 'minimal' sample on boo
  # results in:
  # boo% _HP_DLDOPTS=-warnings ./minimal 
  # /usr/lib/dld.sl: (Warning) May become obsoleted:
  # Separate code/data name spaces: "_ZNK8wxColour4IsOkEv" (DATA)
  # /usr/lib/dld.sl: Unresolved symbol: _ZNK8wxColour4IsOkEv (data)  from /opt/TWWfsw/libwxgtk28/lib/gcc44/libwx_gtk2u_core-2.8.sl.0
  CXXFLAGS="-O2 -fno-inline -fno-default-inline"
  CC_LD_RT="-Wl,+s,+b," ;;
*-irix*)
  CC_LD_RT="-Wl,-rpath," ;;
i?86*-linux*)
  CFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CXXFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CC_LD_RT="-Wl,-rpath,"

