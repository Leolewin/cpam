<?xml version="1.0"?>
<packages>
  <package name="gtk-doc" version="1.16">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>API Documentation Generation Tool</title>
      <vendor>Damon Chaplin</vendor>

      <description>
GTK-Doc is used to document C code. It is typically used to document
the public API of libraries, such as the GTK+ and Gnome libraries.
      </description>

      <install-name>gtkdoc116</install-name>

      <pkgname-base>TWWgtkdoc116</pkgname-base>

      <version>1.16</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWopenjade13"
          subpkg="runtime">v&gt;=1.3.2 r&gt;=5</depend>
        <depend pkgname-base="TWWpython26"
          subpkg="runtime">v&gt;=2.6.7</depend>
        <depend pkgname-base="TWWvim73"
          subpkg="runtime">v&gt;=7.3</depend>
      </subpkg>

      <replace pkgname-base="TWWgtkdoc19"
        install-name="gtkdoc19"/>
      <replace pkgname-base="TWWgtkdoc14"
        install-name="gtkdoc14"/>
      <replace pkgname-base="TWWgtkdoc12"
        install-name="gtkdoc12"/>
    </package-manager>

