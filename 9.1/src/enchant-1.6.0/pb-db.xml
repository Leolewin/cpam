          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <replace pkgname-base="TWWlibenchant15"
        install-name="libenchant15"/>
      <replace pkgname-base="TWWlibenchant13"
        install-name="libenchant13"/>
    </package-manager>

