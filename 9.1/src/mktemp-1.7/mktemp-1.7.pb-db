<packages>
  <package name="mktemp" version="1.7">
    <package-manager name="depot">
      <category>Applications/File</category>
      <title>Secure temporary file creation utility</title>
      <vendor>Todd Miller</vendor>

      <description>
Mktemp is a simple utility designed to make temporary file handling
in shells scripts be safe and simple.
      </description>

      <install-name>mktemp17</install-name>

      <pkgname-base>TWWmktemp17</pkgname-base>

      <version>1.7</version>
      <revision>1</revision>

      <replace pkgname-base="TWWmktemp15"
        install-name="mktemp15"/>
    </package-manager>

