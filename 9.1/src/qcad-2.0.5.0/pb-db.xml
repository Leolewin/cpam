<?xml version="1.0"?>
<packages>
  <package name="qcad" version="2.0.5.0" revision="2">
    <package-manager name="depot">
      <category>Applications/Multimedia</category>
      <title>QCad 2-D CAD package</title>
      <vendor>RibbonSoft, CH</vendor>

      <description>
QCad is an application for computer aided drafting in two dimensions.
With QCad you can create technical drawings such as plans for
buildings, interiors or mechanical parts.
      </description>

      <install-name>qcad20</install-name>

      <pkgname-base>TWWqcad20</pkgname-base>

      <version>2.0.5.0</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWlibqt33"
          subpkg="runtime">v&gt;=3.3.8 r&gt;=3</depend>
      </subpkg>

      <changelog>
        <change date="2012 January 27" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gcc dependency from 4.2.4 to
4.4.6</para></item>
          <item><para>Update qt dependency to latest
revision of 3.3.8</para></item>
        </change>
      </changelog>
    </package-manager>

