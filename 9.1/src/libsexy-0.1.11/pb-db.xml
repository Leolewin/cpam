<?xml version="1.0"?>
<packages>
  <package name="libsexy" version="0.1.11" revision="2">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>Funky fresh graphical widgets for GTK+ 2</title>
      <vendor>Christian Hammond, David Trowbridge, et.al.</vendor>

      <description>
libsexy is a collection of GTK+ widgets that extend the functionality
of such standard widgets as GtkEntry and GtkLabel by subclassing them
and working around the limitations of the widgets.
      </description>

      <install-name>libsexy01</install-name>

      <pkgname-base>TWWlibsexy01</pkgname-base>

      <version>0.1.11</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibxml27"
          subpkg="runtime">v&gt;=2.7.8</depend>
        <depend pkgname-base="TWWlibgtk222"
          subpkg="runtime">v&gt;=2.22.1</depend>
      </subpkg>

      <changelog>
        <change date="2012 January 31" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gtk+ dependency from 2.12.11 to
2.22.1</para></item>
          <item><para>Update libxml dependency from 2.6.32 to
2.7.8</para></item>
        </change>
      </changelog>
    </package-manager>

