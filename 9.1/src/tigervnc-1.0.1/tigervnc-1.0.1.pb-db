<?xml version="1.0"?>
<packages>
  <package name="tigervnc" version="1.0.1">
    <package-manager name="depot">
      <category>User Interface/Desktops</category>
      <title>TigerVNC Network Server</title>
      <vendor>TigerVNC contributors</vendor>

      <description>
Virtual Network Computing (VNC) is a remote display system which
allows you to view a computing "desktop" environment not only on the
machine where it is running, but from anywhere on the Internet and
from a wide variety of machine architectures. This package contains a
client which will allow you to connect to other desktops running a VNC
server.
      </description>

      <install-name>tigervnc10</install-name>

      <pkgname-base>TWWtigervnc10</pkgname-base>

      <version>1.0.1</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWbzip210"
          subpkg="runtime">v&gt;=1.0.6</depend>
        <depend pkgname-base="TWWgettext018"
          subpkg="runtime">v&gt;=0.18.1.1</depend>
        <depend pkgname-base="TWWlibjpeg8"
          subpkg="runtime">v&gt;=8b</depend>
        <depend pkgname-base="TWWlibttf24"
          subpkg="runtime">v&gt;=2.4.6</depend>
        <depend pkgname-base="TWWlibz12"
          subpkg="runtime">v&gt;=1.2.5</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="runtime">v&gt;=5.12.2</depend>
      </subpkg>
    </package-manager>

