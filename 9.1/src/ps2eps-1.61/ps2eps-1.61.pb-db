<?xml version="1.0"?>
<packages>
  <package name="ps2eps" version="1.61" revision="5">
    <package-manager name="depot">
      <category>Applications/Publishing</category>
      <title>Postscript to EPS converter - ps2eps</title>
      <vendor>Dr.-Ing. Roland Bless</vendor>

      <description>
ps2eps is a tool to produce Encapsulated PostScript Files (EPS/EPSF)
from usual one-paged Postscript documents. It calculates correct
Bounding Boxes for those EPS files and filters some special postscript
command sequences that can produce erroneous results on printers.
      </description>

      <install-name>ps2eps16</install-name>

      <pkgname-base>TWWps2eps16</pkgname-base>

      <version>1.61</version>
      <revision>5</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWghostscript90"
          subpkg="runtime">v&gt;=9.01</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="runtime">v&gt;=5.12.2</depend>
      </subpkg>

      <changelog>
        <change date="2012 January 31" revision="5"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update ghostscript dependency from 8.63 to
9.01</para></item>
          <item><para>Updated perl dependency from 5.8.8 to
5.12.2</para></item>
        </change>

        <change date="2009 October 12" revision="4"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update ghostscript dependency from 8.57 to
8.63</para></item>
          <item><para>Update perl dependency from 5.8.6 to
5.8.8</para></item>
        </change>

        <change date="2008 January 23" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update ghostscript dependency from 8.54 to
8.57</para></item>
        </change>

        <change date="2007 March 28" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update ghostscript dependency from 7.07 to
8.54</para></item>
        </change>
      </changelog>
    </package-manager>

