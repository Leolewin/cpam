]]>
      </install>

      <uninstall>
<![CDATA[
rm $_install_prefix/fchksum.so
rm -f $_install_prefix/python_fchksum-${SB_PROG_VER}-py*.egg-info
]]>
      </uninstall>
    </module>

    <module name="2.6.7">
      <install-name>python26p</install-name>

      <dependencies replace="0">
        <depend program="python" var="PYTHON" install-name="python26"
          add-path="prepend">v&gt;=2.6.7</depend>
