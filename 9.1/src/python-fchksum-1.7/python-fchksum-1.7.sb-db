<?xml version="1.0"?>
<!DOCTYPE programs SYSTEM "sb-db.dtd">
<programs>
  <program name="python-fchksum" version="1.7" revision="14">
    <build-name>${SB_PROG_NAME}-${SB_PROG_VER}</build-name>

    <sources>
      <source checksum="0d35b64eb2c99e6463025611cdc081a8"
        checksum-type="md5" size="26678"
        path="src/python-fchksum-1.7.tar.gz"/>
      <patch checksum="71ddc8e3c27590a385ab69c30732c973"
        checksum-type="md5" size="1728"
        path="src/python-fchksum-1.7.tww.patch"/>
    </sources>

    <dependencies>
      <depend program="zlib" var="LIBZ"
        install-name="libz12">v&gt;=1.2.5</depend>
      <depend program="gcc" install-name="gcc44"
        add-path="prepend">v&gt;=4.4.6</depend>
    </dependencies>

    <script-header>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix*)
  CC_LD_RT="-Wl,-blibpath:"
  CC_LD_RT_POST=":/usr/lib" ;;
ia64*-hpux*)
  CC_LD_RT="-Wl,+b," ;;
*-hpux*)
  CC_LD_RT="-Wl,+s,+b," ;;
*-freebsd*|*-linux*)
  CC_LD_RT="-Wl,-rpath," ;;
*-irix*|*-osf*)
  CC_LD_RT="-Wl,-rpath," ;;
*-solaris*)
  CC_LD_RT="-Wl,-R" ;;
esac
]]>
    </script-header>

    <build>
<![CDATA[
LDFLAGS="${SB_VAR_LIBZ+${CC_LD_RT}${SB_VAR_LIBZ_LIB}\
${CC_LD_RT_POST+${CC_LD_RT_POST}}}" ${SB_VAR_PYTHON}/bin/python \
setup.py ${SB_VAR_LIBZ+--zlib-prefix=${SB_VAR_LIBZ}} \
build_ext --inplace
]]>
    </build>

    <install>
<![CDATA[
LDFLAGS="${SB_VAR_LIBZ+${CC_LD_RT}${SB_VAR_LIBZ_LIB}\
${CC_LD_RT_POST+${CC_LD_RT_POST}}}" ${SB_VAR_PYTHON}/bin/python \
setup.py ${SB_VAR_LIBZ+--zlib-prefix=${SB_VAR_LIBZ}} \
install --prefix=${SB_INSTALL_PREFIX}
]]>
    </install>

    <uninstall>
<![CDATA[
rm $_install_prefix/fchksum.so
rm -f $_install_prefix/python_fchksum-${SB_PROG_VER}-py*.egg-info
]]>
    </uninstall>

    <module name="2.7.2">
      <install-name>python27p</install-name>

      <dependencies replace="0">
        <depend program="python" var="PYTHON" install-name="python27"
          add-path="prepend">v&gt;=2.7.2</depend>
      </dependencies>

      <script-header replace="0">
<![CDATA[
_install_prefix="${SB_INSTALL_PREFIX_LIB}/python2.7/site-packages"
]]>
      </script-header>

      <build>
<![CDATA[
LDFLAGS="${SB_VAR_LIBZ+${CC_LD_RT}${SB_VAR_LIBZ_LIB}\
${CC_LD_RT_POST+${CC_LD_RT_POST}}}" ${SB_VAR_PYTHON}/bin/python \
setup.py ${SB_VAR_LIBZ+--zlib-prefix=${SB_VAR_LIBZ}} \
