<?xml version="1.0"?>
<packages>
  <package name="docbook-utils" version="1.0" revision="4">
    <package-manager name="depot">
      <category>Applications/Publishing</category>
      <title>Utilities to convert DocBook (docbook-utils)</title>
      <vendor>Redhat, Inc.</vendor>

      <description attribution="freshmeat.net">
Docbook-utils contains scripts for easy conversion from DocBook SGML
files to other formats (for example, HTML, RTF, and PostScript), and
for comparing SGML files.
      </description>

      <install-name>docbookutils10</install-name>

      <pkgname-base>TWWdbutl10</pkgname-base>

      <version>1.0</version>
      <revision>4</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlynx28"
          subpkg="runtime">v&gt;=2.8.7</depend>
        <depend pkgname-base="TWWmktemp17"
          subpkg="runtime">v&gt;=1.7</depend>
        <depend pkgname-base="TWWopenjad13"
          systype="*-hpux10*"
          subpkg="runtime">v&gt;=1.3.2 r&gt;=5</depend>
        <depend pkgname-base="TWWopenjade13"
          systype="!*-hpux10*"
          subpkg="runtime">v&gt;=1.3.2 r&gt;=5</depend>
        <depend pkgname-base="TWWxmlcat11"
          subpkg="runtime">v&gt;=1.1 r&gt;=5</depend>
      </subpkg>

      <changelog>
        <change date="2011 May 12" revision="4"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Rebuild perl script with perl 5.12.2</para></item>
          <item><para>Update lynx dependency from 2.8.6 to
2.8.7</para></item>
        </change>

        <change date="2009 December 11" revision="3"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Rebuild perl script with perl 5.8.8</para></item>
          <item><para>Update lynx dependency from 2.8.5 to
2.8.6</para></item>
        </change>

        <change date="2006 November 27" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add dependency on mktemp and lynx for docbook2txt 
utility</para></item>
        </change>
      </changelog>
    </package-manager>

