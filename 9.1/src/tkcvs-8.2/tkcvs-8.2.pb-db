<?xml version="1.0"?>
<packages>
  <package name="tkcvs" version="8.2" revision="2">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>Tcl/Tk Interface to CVS</title>
      <vendor>Del</vendor>

      <description>
TkCVS is a Tcl/Tk-based graphical interface to the CVS configuration
management system. It displays the status of the files in the current
working directory, and provides buttons and menus to execute CVS
commands on the selected files. TkDiff is included for browsing and
merging your changes.
      </description>

      <install-name>tkcvs82</install-name>

      <pkgname-base>TWWtkcvs82</pkgname-base>

      <version>8.2</version>
      <revision>2</revision>

      <config>lib/site_def</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWcvs112"
          subpkg="runtime">v&gt;=1.12.13</depend>
        <depend pkgname-base="TWWtk85"
          subpkg="runtime">v&gt;=8.5.10</depend>
      </subpkg>

      <replace pkgname-base="TWWtkcvs80"
        install-name="tkcvs80"/>
      <replace pkgname-base="TWWtkcvs72"
        install-name="tkcvs72"/>

      <changelog>
        <change date="2012 March 1" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update cvs dependency from 1.11.23 to
1.12.13</para></item>
          <item><para>Update tcl/tk dependency from 8.5.6 to
8.5.10</para></item>
        </change>
      </changelog>
    </package-manager>

