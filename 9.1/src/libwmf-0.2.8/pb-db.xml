    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>WMF Graphics Library</title>
      <vendor>Caolan McNamara</vendor>

      <description attribution="The OpenPKG Project">
libwmf is a library for reading vector images in native Windows
Metafile Format (WMF) and for either (a) displaying them in, e.g., an
X window; or (b) converting them to more standard/open file formats
such as, e.g., the W3C's XML-based Scaleable Vector Graphic (SVG)
format.
      </description>

      <install-name>libwmf02</install-name>

      <pkgname-base>TWWlibwmf02</pkgname-base>

      <version>0.2.8</version>
      <revision>8</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWexpat20"
          subpkg="runtime">v&gt;=2.0.1</depend>
        <depend pkgname-base="TWWlibjpeg8"
          subpkg="runtime">v&gt;=8b</depend>
        <depend pkgname-base="TWWlibpng14"
          subpkg="runtime">v&gt;=1.4.4</depend>
        <depend pkgname-base="TWWlibttf24"
          subpkg="runtime">v&gt;=2.4.6</depend>
        <depend pkgname-base="TWWlibz12"
          subpkg="runtime">v&gt;=1.2.5</depend>
        <depend pkgname-base="TWWplotutils26"
          subpkg="runtime">v&gt;=2.6</depend>
        <depend pkgname-base="TWWxpm"
          subpkg="runtime">v&gt;=3.4k</depend>
      </subpkg>

      <replace pkgname-base="TWWlibwmf01"
        install-name="libwmf01"/>

