<?xml version="1.0"?>
<packages>
  <package name="recode" version="3.6" revision="4">
    <package-manager name="depot">
      <category>Applications/Text</category>
      <title>Recode character set recoder</title>
      <vendor>Francois Pinard</vendor>

      <description attribution="The OpenPKG Project">
Recode converts files between various character sets and surfaces. It
recognises or produces more than 300 different character sets and
transliterates files between almost any pair.
      </description>

      <install-name>recode36</install-name>

      <pkgname-base>TWWrecode36</pkgname-base>

      <version>3.6</version>
      <revision>4</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgettext018"
          subpkg="runtime">v&gt;=0.18.1.1</depend>
      </subpkg>

      <replace pkgname-base="TWWrecode35"
        install-name="recode35"/>

      <changelog>
        <change date="2012 January 25" revision="4"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gettext dependency from 0.17 to
0.18.1.1</para></item>
        </change>

        <change date="2009 December 7" revision="3"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gettext dependency from 0.14.1 to 
0.17</para></item>
        </change>

        <change date="2004 November 2" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Upgrade gettext dependency from 0.10.4 to
0.14.1</para></item>
        </change>
      </changelog>
    </package-manager>

