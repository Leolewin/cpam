        <depend pkgname-base="TWW.libapr14"
          subpkg="runtime">v&gt;=1.4.5.1</depend>
        <depend pkgname-base="TWW.libdb47"
          subpkg="runtime">v&gt;=4.7.25.1</depend>
        <depend pkgname-base="TWW.libexpat20"
          subpkg="runtime">v&gt;=2.0.1.1</depend>
        <depend pkgname-base="TWW.libiconv113"
          subpkg="runtime">v&gt;=1.13.1.1</depend>
        <depend pkgname-base="TWW.openldap24"
          subpkg="runtime">v&gt;=2.4.26.1</depend>
      </subpkg>

      <replace pkgname-base="TWW.libaprutil12"
        install-name="libaprutil12"/>
    </package-manager>

    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>APR-UTIL - Apache Portable Runtime Utility library</title>
      <vendor>Apache Software Foundation</vendor>

      <description>
APR-util provides a number of helpful abstractions on top of APR.
