<?xml version="1.0"?>
<packages>
  <package name="apr-util" version="1.3.12">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>APR-UTIL - Apache Portable Runtime Utility library</title>
      <vendor>Apache Software Foundation</vendor>

      <description>
APR-util provides a number of helpful abstractions on top of APR.
      </description>

      <install-name>libaprutil13</install-name>

      <pkgname-base>TWWlibaprutil13</pkgname-base>

      <version>1.3.12</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWexpat20"
          subpkg="runtime">v&gt;=2.0.1</depend>
        <depend pkgname-base="TWWldap24"
          subpkg="runtime">v&gt;=2.4.26</depend>
        <depend pkgname-base="TWWlibapr14"
          subpkg="runtime">v&gt;=1.4.5</depend>
        <depend pkgname-base="TWWlibdb47"
          subpkg="runtime">v&gt;=4.7.25</depend>
        <depend pkgname-base="TWWlibiconv113"
          subpkg="runtime">v&gt;=1.13.1</depend>
      </subpkg>

      <replace pkgname-base="TWWlibapru12"
        install-name="libaprutil12"/>
    </package-manager>

    <package-manager name="inst">
      <category>System Environment/Libraries</category>
      <title>APR-UTIL - Apache Portable Runtime Utility library</title>
      <vendor>Apache Software Foundation</vendor>

      <description>
APR-util provides a number of helpful abstractions on top of APR.
      </description>

      <install-name>libaprutil13</install-name>

      <pkgname-base>TWWlibaprutil13</pkgname-base>

      <version>2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibapr14"
          subpkg="runtime">v&gt;=1</depend>
        <depend pkgname-base="TWWlibdb47"
          subpkg="runtime">v&gt;=1</depend>
        <depend pkgname-base="TWWlibexpat20"
          subpkg="runtime">v&gt;=1</depend>
        <depend pkgname-base="TWWlibiconv113"
          subpkg="runtime">v&gt;=1</depend>
        <depend pkgname-base="TWWopenldap24"
          subpkg="runtime">v&gt;=2</depend>
      </subpkg>

      <replace pkgname-base="TWWlibaprutil12"
        install-name="libaprutil12"/>
    </package-manager>

    <package-manager name="lpp">
      <category>System Environment/Libraries</category>
      <title>APR-UTIL - Apache Portable Runtime Utility library</title>
      <vendor>Apache Software Foundation</vendor>

      <description>
APR-util provides a number of helpful abstractions on top of APR.
