<packages>
  <package name="iso-codes" version="3.25">
    <package-manager name="depot">
      <category>System Environment/Base</category>
      <title>ISO code lists and translations</title>
      <vendor>Alastair McKinstry</vendor>

      <description>
This package aims to provide the list of the country and language (and
currency) names in one place, rather than repeated in many programs.
      </description>

      <install-name>isocodes32</install-name>

      <pkgname-base>TWWisocodes32</pkgname-base>

      <version>3.25</version>
      <revision>1</revision>

      <replace pkgname-base="TWWisocode10"
        install-name="isocodes10"/>
    </package-manager>

