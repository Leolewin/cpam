<?xml version="1.0"?>
<packages>
  <package name="a2ps" version="4.14" revision="2">
    <package-manager name="depot">
      <category>Applications/Publishing</category>
      <title>GNU a2ps Postscript filter</title>
      <vendor>Akim Demaille, Miguel Santana</vendor>

      <description>
a2ps is an Any to PostScript filter. It converts files of multiple
types to PostScript, provided the necessary tools are available.
Among the supported file types are text, PostScript, texinfo, TeX,
etc.
      </description>

      <install-name>a2ps41</install-name>

      <pkgname-base>TWWa2ps41</pkgname-base>

      <version>4.14</version>
      <revision>2</revision>

      <config>etc/a2ps.cfg</config>
      <config>etc/a2ps-site.cfg</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgettext018"
          subpkg="runtime">v&gt;=0.18.1.1</depend>
      </subpkg>

      <replace pkgname-base="TWWa2ps"
        install-name="a2ps"/>

      <changelog>
        <change date="2011 May 5" revision="2"
        author="Gary V. Vaughan" email="gary@thewrittenword.com">
          <item><para>Update gettext dependency from 0.17 to
0.18.1.1</para></item>
        </change>
      </changelog>
    </package-manager>

