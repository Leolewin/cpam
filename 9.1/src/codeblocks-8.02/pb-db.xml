    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>Code Blocks IDE</title>
      <vendor>Yiannis Mandravellos, et. al.</vendor>

      <description>
Code::Blocks is a free C++ IDE built to meet the most demanding needs
of its users. It is designed to be very extensible and fully
configurable.
      </description>

      <install-name>codeblocks80</install-name>

      <pkgname-base>TWWcodeblocks80</pkgname-base>

      <version>8.02</version>
      <revision>2</revision>

      <variable>
        <path name="GTKTHEMES_PATH"
          install-name="gtkthemes222"/>
        <path name="XDGDATA_DATADIR_PATH"
          install-name="xdgdata12">share</path>
      </variable>

      <wrapper name="cb_share_config" path="pkg/codeblocks-wrapper.c"
        checksum="acd0df31ea420a99dea4c0bd39435e7b"
        checksum-type="md5" size="1956"/>
      <wrapper name="codeblocks" path="pkg/codeblocks-wrapper.c"
        checksum="acd0df31ea420a99dea4c0bd39435e7b"
        checksum-type="md5" size="1956"/>
      <wrapper name="codesnippets" path="pkg/codeblocks-wrapper.c"
        checksum="acd0df31ea420a99dea4c0bd39435e7b"
        checksum-type="md5" size="1956"/>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgcc44"
          subpkg="libruntime">v&gt;=4.4.6</depend>
        <depend pkgname-base="TWWwxgtk28"
          subpkg="runtime">v&gt;=2.8.12</depend>
        <depend pkgname-base="TWWxdgdata12"
          subpkg="runtime">v&gt;=1.2</depend>
      </subpkg>

      <changelog>
        <change date="2012 January 18" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update gcc dependency from 4.2.4 to 
4.4.6</para></item>
          <item><para>Update wxGTK dependency from 2.8.10 to 
2.8.12</para></item>
          <item><para>Update xdgdata dependency from 1.1 to 
1.2</para></item>
        </change>
      </changelog>
    </package-manager>

