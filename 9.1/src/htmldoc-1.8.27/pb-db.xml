  <package name="htmldoc" version="1.8.27" revision="4">
    <package-manager name="depot">
      <category>Applications/Publishing</category>
      <title>HTML to PDF/PS converter</title>
      <vendor>Easy Software Products</vendor>

      <description>
HTMLDOC converts HTML source files into indexed HTML, PostScript, or
Portable Document Format (PDF) files that can be viewed online or
printed.
      </description>

      <install-name>htmldoc18</install-name>

      <pkgname-base>TWWhtdoc18</pkgname-base>

      <version>1.8.27</version>
      <revision>4</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWfltk11"
          subpkg="runtime">v&gt;=1.1.9</depend>
        <depend pkgname-base="TWWlibopenssl10"
          subpkg="runtime">v&gt;=1.0.0e</depend>
        <depend pkgname-base="TWWxpm"
          subpkg="runtime">v&gt;=3.4k</depend>
      </subpkg>

      <changelog>
        <change date="2012 February 1" revision="4"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update openssl dependency from 0.9.8h to
1.0.0e</para></item>
        </change>

        <change date="2009 June 22" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update fltk dependency from 1.1.7 to
1.1.9</para></item>
          <item><para>Update openssl dependency from 0.9.8e to
0.9.8h</para></item>
        </change>

        <change date="2007 September 28" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Update openssl dependency from 0.9.7b to
0.9.8e</para></item>
        </change>
      </changelog>
    </package-manager>

