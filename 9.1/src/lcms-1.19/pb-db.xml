<?xml version="1.0"?>
<packages>
  <package name="lcms" version="1.19">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>LCMS Color Management Library</title>
      <vendor>Marti Maria</vendor>

      <description>
Little cms intends to be a small-footprint, speed optimized color
management engine.
      </description>

      <install-name>liblcms11</install-name>

      <pkgname-base>TWWlcms11</pkgname-base>

      <version>1.19</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibjpeg8"
          subpkg="runtime">v&gt;=8b</depend>
        <depend pkgname-base="TWWlibtiff39"
          subpkg="runtime">v&gt;=3.9.5</depend>
        <depend pkgname-base="TWWlibz12"
          subpkg="runtime">v&gt;=1.2.5</depend>
      </subpkg>
    </package-manager>

