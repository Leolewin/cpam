<?xml version="1.0"?>
<packages>
  <package name="libidn" version="1.21">
    <package-manager name="depot">
      <category>Development/Libraries</category>
      <title>Internationalized domain name library</title>
      <vendor>Simon Josefsson</vendor>

      <description>
Libidn is a package for internationalized string handling based on the
Stringprep, Punycode, IDNA and TLD specifications.
      </description>

      <install-name>libidn121</install-name>

      <pkgname-base>TWWlibidn121</pkgname-base>

      <version>1.21</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgettext018"
          subpkg="runtime">v&gt;=0.18.1.1</depend>
      </subpkg>

      <replace pkgname-base="TWWlibidn11"
        install-name="libidn11"/>
      <replace pkgname-base="TWWlibidn10"
        install-name="libidn10"/>

      <changelog>
        <change date="2012 April 3" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Add 64bit libraries</para></item>
        </change>
      </changelog>
    </package-manager>

