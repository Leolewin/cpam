<?xml version="1.0"?>
<packages>
  <package name="xmlcatalog" version="1.1" revision="8">
    <package-manager name="depot">
      <category>Applications/Publishing</category>
      <title>XML catalog files for Docbook</title>
      <vendor>3rd Parties</vendor>

      <description>
Various support files for Docbook use:
  * Docbook DTDs: 3.1, 4.0, 4.1, 4.2, 4.3, 4.4, 4.5
  * Docbook XML DTDs: 4.0, 4.1.2, 4.2, 4.3, 4.4, 4.5
  * Docbook DSSSL stylesheets
  * Docbook XSL stylesheets
  * SGML 8879.1986 ISO entities
  * XML 8879.1986 ISO entities
      </description>

      <install-name>xmlcatalog11</install-name>

      <pkgname-base>TWWxmlcat11</pkgname-base>

      <version>1.1</version>
      <revision>8</revision>

      <changelog>
        <change date="2010 December 21" revision="8"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update docbook-xsl from 1.74.1 to
1.76.1</para></item>
        </change>

        <change date="2009 February 18" revision="7"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Add Docbook SGML DTD 4.5</para></item>
          <item><para>Add Docbook XML DTD 4.5</para></item>
          <item><para>Update docbook-xsl from 1.65.1 to
1.74.1</para></item>
          <item><para>Update docbook-dsssl from 1.78 to
1.79</para></item>
        </change>

        <change date="2005 September 11" revision="6"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add Docbook SGML DTD 4.4</para></item>
          <item><para>Add Docbook XML DTD 4.4</para></item>
        </change>

        <change date="2005 December 31" revision="5"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Forgot to include cals-tbl.dtd for Docbook SGML
4.0</para></item>
        </change>

        <change date="2005 November 19" revision="4"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Add module for glade 2.0 DTD</para></item>
          <item><para>Add module for scrollkeeper 1.1 OMF
DTD</para></item>
        </change>

        <change date="2005 July 25" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Fix rewriteURI and rewriteSystem paths for
docbook-xsl</para></item>
        </change>

        <change date="2005 March 9" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Forgot to install htmltblx.mod for
docbook-sgml-4.3 and docbook-xml-4.3 modules</para></item>
        </change>
      </changelog>
    </package-manager>

