        add-path="prepend" type="build">v&gt;=2.4.3</depend>
      <depend var="TETEXP" install-name="tetex30p"
        set-var-only="1"/>
      <depend program="Xaw3d" var="LIBXAW3D"
        install-name="libxaw3d15">v&gt;=1.5e</depend>
      <depend program="flex" install-name="flex25"
        add-path="prepend">v&gt;=2.5.35</depend>
      <depend program="gd" var="LIBGD"
        install-name="libgd20">v&gt;=2.0.35</depend>
