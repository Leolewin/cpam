    <script-header>
<![CDATA[
_docdir="${SB_INSTALL_PREFIX}/doc"
]]>
    </script-header>

    <configure>
<![CDATA[
case "${SB_SYSTYPE}" in
*-aix4*)
  CFLAGS="-O2 -mcpu=powerpc"
  CXXFLAGS="-O2 -mcpu=powerpc"
  LDFLAGS="-Wl,-brtl -Wl,-blibpath:/usr/lib"
  CC_LD_RT="-Wl,-blibpath:" ;;
*-aix*)
  CC=xlc CFLAGS="-O2 -qro -qroconst -qmaxmem=-1 -qarch=ppc"
  CXX=xlC CXXFLAGS="-O2 -qro -qroconst -qmaxmem=-1 -qarch=ppc"
  LDFLAGS="-Wl,-brtl -Wl,-blibpath:/usr/lib"
  CC_LD_RT="-Wl,-blibpath:" ;;
ia64*-hpux*)
  CC=cc CFLAGS="-z +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-z +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CPPFLAGS="-I/usr/contrib/X11R6/include"
  LDFLAGS="-L/usr/contrib/X11R6/lib"
  CC_LD_RT="-Wl,+s,+b," ;;
*-hpux11.[01]*)
  CC=cc CFLAGS="+O2 +ESlit +Ofltacc +DAportable +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="+O2 -AA -z +Ofltacc +ESlit +DAportable \
+Oentrysched +Odataprefetch +Onolimit"
  CPPFLAGS="-I/usr/contrib/X11R6/include"
  LDFLAGS="-L/usr/contrib/X11R6/lib"
  CC_LD_RT="-Wl,+s,+b," ;;
*-hpux10*)
  CFLAGS="-O2 -march=1.1" CXXFLAGS="-O2 -march=1.1"
  CPPFLAGS="-I/usr/contrib/X11R6/include"
  LDFLAGS="-L/usr/contrib/X11R6/lib"
  CC_LD_RT="-Wl,+s,+b," ;;
*-hpux*)
  CC=cc CFLAGS="+O2 +Olit=all +Ofltacc +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="+O2 -AA -z +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CPPFLAGS="-I/usr/contrib/X11R6/include"
  LDFLAGS="-L/usr/contrib/X11R6/lib"
  CC_LD_RT="-Wl,+s,+b," ;;
*-irix*)
  CC=cc CFLAGS="-Wl,-woff,84 -Wl,-woff,85 -woff 1429 \
-use_readonly_const -G0 -rdata_shared -DEBUG:optimize_space=ON \
-O2 -OPT:Olimit=0"
  CXX=CC CXXFLAGS="-Wl,-woff,84 -Wl,-woff,85 -woff 1429 \
-use_readonly_const -G0 -rdata_shared -DEBUG:optimize_space=ON \
-ptused -FE:template_in_elf_section -O2 -OPT:Olimit=0"
  CC_LD_RT="-Wl,-rpath," ;;
i?86*-linux*)
  CFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CXXFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--x-libraries=/usr/X11R6/lib" ;;
x86_64*-linux*)
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--x-libraries=/usr/X11R6/lib64" ;;
*-linux*)
  CC_LD_RT="-Wl,-rpath," ;;
*-osf*)
  CC=cc CFLAGS="-O2 -arch ev5 -msym -readonly_strings"
  CXX=cxx CXXFLAGS="-O2 -arch ev5 -msym -readonly_strings"
  CC_LD_RT="-Wl,-rpath," ;;
i?86*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=pentium4"
  CXX=CC CXXFLAGS="-xO2 -xtarget=pentium4"
  LDFLAGS="-L/usr/openwin/lib"
  CC_LD_RT="-R" ;;
*-solaris2.[68])
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 \
-xarch=v8plusa"
  CXX=CC CXXFLAGS="-xO2 -xtarget=ultra2 -xarch=v8plusa"
  LDFLAGS="-L/usr/openwin/lib"
  CC_LD_RT="-R" ;;
*-solaris2.7)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO1 -xtarget=ultra2 \
-xarch=v8plusa"
  CXX=CC CXXFLAGS="-xO1 -xtarget=ultra2 -xarch=v8plusa"
  LDFLAGS="-L/usr/openwin/lib"
  CC_LD_RT="-R" ;;
*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 \
-m32 -xarch=sparcvis"
  CXX=CC CXXFLAGS="-xO2 -xtarget=ultra2 -m32 -xarch=sparcvis"
  LDFLAGS="-L/usr/openwin/lib"
  CC_LD_RT="-R" ;;
esac

# Unpack macros
if [ ! -d "${SB_VAR_TETEXP}" ]; then
  sudo -h >/dev/null 2>&1 || {
    sb_msg_err "'sudo' program not found. Before teTeX can \
be configured, the ${SB_SRC_MACROS} file must be unpacked to \
${SB_VAR_TETEXP}/texmf. 'sudo' is used to run the necessary \
commands as root because the configuration step is usually \
run as a non-root user. Please configure 'sudo' per the sb(1) \
manual and rerun."
    exit 1
  }

  sudo ${SB_PATH_INSTALL} -m 755 -o root -g 0 -d ${SB_VAR_TETEXP}/texmf
  (cd ${SB_VAR_TETEXP}/texmf
  sudo ${SB_PATH_TAR} zxf ${SB_SRC_MACROS}
  sudo ${SB_PATH_CHOWN} -hR 0:0 ${SB_VAR_TETEXP}/texmf)
fi

CC="${CC:-gcc}" CFLAGS="${CFLAGS:--O2}" \
CPPFLAGS="${SB_VAR_LIBXAW3D+-I${SB_VAR_LIBXAW3D_INC}} \
${SB_VAR_XPM+-I${SB_VAR_XPM_INC}} \
${SB_VAR_LIBTRIO+-I${SB_VAR_LIBTRIO_INC}} \
${CPPFLAGS+${CPPFLAGS}}" CXX="${CXX:-g++}" \
CXXFLAGS="${CXXFLAGS:--O2}" \
LDFLAGS="${SB_VAR_LIBXAW3D+-L${SB_VAR_LIBXAW3D_LIB}} \
${SB_VAR_ZLIB+-L${SB_VAR_ZLIB_LIB}} \
${SB_VAR_XPM+-L${SB_VAR_XPM_LIB}} \
${SB_VAR_LIBTRIO+-L${SB_VAR_LIBTRIO_LIB}} \
${SB_VAR_LIBXAW3D+${CC_LD_RT}${SB_VAR_LIBXAW3D_LIB}} \
${SB_VAR_T1LIB+${CC_LD_RT}${SB_VAR_T1LIB_LIB}} \
${SB_VAR_LIBGD+${CC_LD_RT}${SB_VAR_LIBGD_LIB}} \
${SB_VAR_ZLIB+${CC_LD_RT}${SB_VAR_ZLIB_LIB}} \
${SB_VAR_XPM+${CC_LD_RT}${SB_VAR_XPM_LIB}} \
${SB_VAR_LIBPNG+${CC_LD_RT}${SB_VAR_LIBPNG_LIB}} \
${SB_VAR_GCC_RT+${CC_LD_RT}${SB_VAR_GCC_RT}/lib} \
${SB_VAR_LIBTRIO+${CC_LD_RT}${SB_VAR_LIBTRIO_LIB}} \
${LDFLAGS+${LDFLAGS}}" INSTALL="${SB_PATH_INSTALL} -c" \
${SHELL} ./configure --enable-ipc --without-texinfo \
--without-dialog --without-t1utils --without-psutils \
--disable-multiplatform --without-cxx-runtime-hack \
--datadir=${SB_VAR_TETEXP} --with-system-ncurses \
--with-system-pnglib --with-system-zlib \
--with-system-t1lib --with-system-gd --enable-shared --enable-static \
${SB_VAR_NCURSES+--with-ncurses-libdir=${SB_VAR_NCURSES_LIB}} \
${SB_VAR_NCURSES+--with-ncurses-include=${SB_VAR_NCURSES_INC}} \
${SB_VAR_LIBPNG+--with-pnglib-libdir=${SB_VAR_LIBPNG_LIB}} \
${SB_VAR_LIBPNG+--with-pnglib-include=${LIBPNG_INC}} \
${SB_VAR_ZLIB+--with-zlib-libdir=${SB_VAR_ZLIB_LIB}} \
${SB_VAR_ZLIB+--with-zlib-include=${SB_VAR_ZLIB_INC}} \
${SB_VAR_T1LIB+--with-t1lib-libdir=${SB_VAR_T1LIB_LIB}} \
${SB_VAR_T1LIB+--with-t1lib-include=${SB_VAR_T1LIB_INC}} \
${SB_VAR_LIBGD+--with-gd-libdir=${SB_VAR_LIBGD_LIB}} \
${SB_VAR_LIBGD+--with-gd-include=${SB_VAR_LIBGD_INC}} \
--with-xdvi-x-toolkit=xaw3d --with-mktexfmt-default \
--without-texi2html --without-psutils --without-texinfo \
--cache-file=/dev/null \
--disable-a4 \
${ARGS+${ARGS}} --libdir=${SB_INSTALL_PREFIX_LIB} \
--prefix=${SB_INSTALL_PREFIX}
]]>
    </configure>

    <build>
<![CDATA[
gmake all
]]>
    </build>

    <install>
<![CDATA[
gmake install

${SB_PATH_CHMOD} 644 ${SB_VAR_TETEXP}/texmf/ls-R

# make parameters usable for passiveTeX
${SB_PATH_SED} -i -e "\
s/main_memory = 1000000/main_memory = 2500000/; \
s/save_size = 5000/save_size = 10000/" \
${SB_VAR_TETEXP}/texmf/web2c/texmf.cnf

# replace perl path
for i in e2pall epstopdf texdoctk; do
  ${SB_PATH_SED} -i -e "\
1 s|#!.*|#!${SB_VAR_PERL}/bin/perl|" ${SB_INSTALL_PREFIX}/bin/$i
done

# install texinfo source files
${SB_PATH_INSTALL} -m 755 -o root -g 0 -d ${_docdir}/texinfo/web2c \
${_docdir}/texinfo/dvipng ${_docdir}/texinfo/kpathsea \
${_docdir}/texinfo/dvips ${_docdir}/texinfo/dvips/contrib \
${_docdir}/texinfo/latex
(cd texk
${SB_PATH_INSTALL} -m 644 -o root -g 0 dvipng/dvipng.help \
dvipng/dvipng.texi dvipng/install.texi dvipng/macros.texi \
dvipng/readme.texi ${_docdir}/texinfo/dvipng
${SB_PATH_INSTALL} -m 644 -o root -g 0 dvipsk/dvips.texi \
${_docdir}/texinfo/dvips
${SB_PATH_INSTALL} -m 644 -o root -g 0 dvipsk/contrib/config.proto \
${_docdir}/texinfo/dvips/contrib
${SB_PATH_INSTALL} -m 644 -o root -g 0 kpathsea/bugs.texi \
kpathsea/hier.texi kpathsea/install.texi kpathsea/kpathsea.texi \
kpathsea/unixtex.texi ${_docdir}/texinfo/kpathsea
${SB_PATH_INSTALL} -m 644 -o root -g 0 web2c/doc/install.texi \
web2c/tests/just.texi web2c/doc/ref.txi web2c/doc/web2c.texi \
${_docdir}/texinfo/web2c
${SB_PATH_INSTALL} -m 644 -o root -g 0 tetex/latex2e.texi \
${_docdir}/texinfo/latex/latex.texi)

rm -rf /var/tmp/texfonts
${SB_INSTALL_PREFIX}/bin/texhash
]]>
    </install>

    <uninstall>
      <remove-db-entry program="jadetex" systype="!*-aix4*">v==3.13</remove-db-entry>
      <remove-db-entry program="prosper" module="3.0">v==1.00.4</remove-db-entry>
    </uninstall>

    <notes>
      <note type="usage">
        <para>The <command>${SB_INSTALL_BASE}/tetex30p</command> contains all TeX
packages. In addition to the standard set of TeX packages, the
following additional packages are available:</para>
        <unorderedlist>
          <item><para><command>prosper</command> LaTeX package for
writing transparencies (documentation available at
<command>${SB_INSTALL_BASE}/tetex30p/texmf/doc/prosper</command>)</para></item>
          <item><para><command>jadetex</command> LaTeX package for
processing the output from OpenJade in TeX (<command>-t</command>)
mode</para></item>
        </unorderedlist>
      </note>

      <platform name="HP-UX 11.00">
        <para>Patch <command>PHSS_22543</command> providing the
<command>libstd_v2</command> Standard C++ library is required as the
HP C++ compiler was used.</para>
      </platform>

      <platform name="Tru64 UNIX">
        <para>The latest C++ runtime library is required due to the
usage of the latest Compaq C++ compiler. It is available from the
following locations:</para>
        <unorderedlist>
          <item><para>package CXXREDIST651V37.tar in the support
directory of the CD</para></item>
          <item><para>online support directory <ulink url="ftp://support.thewrittenword.com/dists">ftp://support.thewrittenword.com/dists</ulink></para></item>
          <item><para>directly from Compaq at <ulink url="ftp://ftp.compaq.com/pub/products/C-CXX/Tru64/cxx/cxxredist.htm">ftp://ftp.compaq.com/pub/products/C-CXX/Tru64/cxx/cxxredist.htm</ulink></para></item>
        </unorderedlist>
      </platform>
    </notes>

    <changelog>
      <change date="2010 December 20" revision="21"
      author="Peter O'Gorman" email="pogma@thewrittenword.com">
        <item><para>Update perl dependency from 5.8.8 to
5.12.2</para></item>
        <item><para>Update ncurses dependency from 5.6 to
5.7</para></item>
        <item><para>Update zlib dependency from 1.2.3 to
1.2.5</para></item>
        <item><para>Update bison dependency from 2.3 to
2.4</para></item>
        <item><para>Update flex dependency from 2.5.4a to
2.5.35</para></item>
