  CXX=xlC CXXFLAGS="-O2 -qro -qroconst -qmaxmem=-1 -qarch=ppc"
  CC_LD_RT="-Wl,-blibpath:"
  LDFLAGS="-Wl,-brtl -Wl,-blibpath:/usr/lib"

  ARGS="--disable-xinerama --enable-ipv6 --disable-gstreamer" ;;
ia64*-hpux*)
  CC=cc CFLAGS="-z +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-AA -z +Ofltacc +O2 +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CPPFLAGS="-I/usr/contrib/X11R6/include"
  LDFLAGS="-L/usr/contrib/X11R6/lib"
  CC_LD_RT="-Wl,+b,"

  ARGS="--disable-xinerama --enable-ipv6 --disable-gstreamer" ;;
*-hpux11.[01]*)
  CC=cc CFLAGS="-z +O2 +Ofltacc +Olit=all +DAportable \
+Oentrysched +Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-AA -z +Ofltacc +O2 +Olit=all +DAportable \
+Oentrysched +Odataprefetch +Onolimit"
  CPPFLAGS="-I/usr/contrib/X11R6/include"
  LDFLAGS="-L/usr/contrib/X11R6/lib"
  CC_LD_RT="-Wl,+s,+b,"

  ARGS="--disable-xinerama --disable-ipv6 --disable-gstreamer" ;;
*-hpux*)
  CC=cc CFLAGS="-z +O2 +Ofltacc +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CXX=aCC CXXFLAGS="-AA -z +Ofltacc +O2 +Olit=all +Oentrysched \
+Odataprefetch +Onolimit"
  CPPFLAGS="-I/usr/contrib/X11R6/include"
  LDFLAGS="-L/usr/contrib/X11R6/lib"
  CC_LD_RT="-Wl,+s,+b,"

  ARGS="--disable-xinerama --enable-ipv6 --disable-gstreamer" ;;
*-irix*)
  CC=c99 CFLAGS="-Wl,-woff,84 -Wl,-woff,85 -woff 1009,1429 \
-use_readonly_const -G0 -rdata_shared -DEBUG:optimize_space=ON \
-O1 -OPT:Olimit=0"
  CXX=CC CXXFLAGS="-Wl,-woff,84 -Wl,-woff,85 -woff 1009,1429 \
-use_readonly_const -G0 -rdata_shared -DEBUG:optimize_space=ON \
-ptused -FE:template_in_elf_section -O1 -OPT:Olimit=0"
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--disable-xinerama --disable-ipv6 --disable-gstreamer" ;;
i?86*-linux*)
  CFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CXXFLAGS="-O2 -mtune=pentiumpro -march=pentiumpro"
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--enable-xinerama --enable-ipv6 --enable-gstreamer" ;;
*-linux*)
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--enable-xinerama --enable-ipv6 --enable-gstreamer" ;;
*-osf*)
  CC=cc CFLAGS="-O2 -arch ev5 -msym -readonly_strings"
  CXX=cxx CXXFLAGS="-O2 -arch ev5 -model ansi -msym \
-readonly_strings -timplicit_local"
  CC_LD_RT="-Wl,-rpath,"

  ARGS="--disable-xinerama --disable-ipv6 --disable-gstreamer" ;;
i?86*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=pentium4"
  CXX=CC CXXFLAGS="-xO2 -xtarget=pentium4"
  LDFLAGS="-L/usr/openwin/lib -R/usr/openwin/lib"
  CC_LD_RT="-R"

  # without -Xc, __STDC__ defined but not defined to 1. set
  # -DANSICPP to get correct SIZEOF() definition
  CPPFLAGS="-DANSICPP -DFUNCPROTO"

  ARGS="--disable-xinerama --enable-ipv6 --enable-gstreamer" ;;
*-solaris2.8)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 \
-xarch=v8plusa"
  CXX=CC CXXFLAGS="-xO2 -xtarget=ultra2 -xarch=v8plusa"
  LDFLAGS="-L/usr/openwin/lib -R/usr/openwin/lib"
  CC_LD_RT="-R"

  # without -Xc, __STDC__ defined but not defined to 1. set
  # -DANSICPP to get correct SIZEOF() definition
  CPPFLAGS="-DANSICPP -DFUNCPROTO"

  ARGS="--enable-ipv6 --disable-xinerama --enable-gstreamer" ;;
*-solaris*)
  CC=cc CFLAGS="-mr -Qn -xstrconst -xO2 -xtarget=ultra2 -m32 \
-xarch=sparcvis"
  CXX=CC CXXFLAGS="-xO2 -xtarget=ultra2 -m32 -xarch=sparcvis"
  LDFLAGS="-L/usr/openwin/lib -R/usr/openwin/lib"
  CC_LD_RT="-R"

  # without -Xc, __STDC__ defined but not defined to 1. set
  # -DANSICPP to get correct SIZEOF() definition
  CPPFLAGS="-DANSICPP -DFUNCPROTO"

  ARGS="--disable-xinerama --enable-ipv6 --enable-gstreamer" ;;
esac
]]>
