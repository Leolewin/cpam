if [ "${SB_VAR_XMLCATALOG+set}" = set ]; then
  ${SB_PATH_SED} -i -e "\
s!/etc/xml/catalog!${SB_VAR_XMLCATALOG}/catalog.xml!;" \
doc/xmlcatalog.1 doc/xmllint.1
fi
]]>
