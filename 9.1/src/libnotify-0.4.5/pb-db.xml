<packages>
  <package name="libnotify" version="0.4.5" revision="2">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>Libnotify Notification Library</title>
      <vendor>Christian Hammond, et.al.</vendor>

      <description attribution="The Debian project">
A library that sends desktop notifications to a notification daemon,
as defined in the Desktop Notifications spec. These notifications can
be used to inform the user about an event or display some form of
information without getting in the user's way.
      </description>

      <install-name>libnotify04</install-name>

      <pkgname-base>TWWnotify04</pkgname-base>

      <version>0.4.5</version>
      <revision>2</revision>

      <variable>
        <path name="PYTHON_26_INCLUDE_PATHS_D"
          install-name="python26">etc/include.paths.d</path>
        <path name="PYTHON_27_INCLUDE_PATHS_D"
          install-name="python27">etc/include.paths.d</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWdbus12"
          subpkg="runtime">v&gt;=1.2.26</depend>
        <depend pkgname-base="TWWlibgtk222"
          subpkg="runtime">v&gt;=2.22.1</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="ab92ad49ccb40db9f3f5d287cc762594"
          checksum-type="md5" size="10097"/>
        <preremove path="pkg/remove-from-include.paths-hpux"
          checksum="07fa380e3c03cf37fb551504839f2e2c"
          checksum-type="md5" size="6917"/>
      </subpkg>

      <changelog>
        <change date="2011 March 21" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update dbus dependency from 1.2.12 to
1.2.26</para></item>
          <item><para>Update gtk+ dependency from 2.12.11 to
2.22.1</para></item>
        </change>
      </changelog>
    </package-manager>

