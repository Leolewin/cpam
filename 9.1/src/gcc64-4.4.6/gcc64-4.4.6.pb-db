<?xml version="1.0"?>
<packages>
  <package name="gcc64" version="4.4.6">
    <package-manager name="depot">
      <category>Development/Languages</category>
      <title>GNU Compiler Collection</title>
      <vendor>Free Software Foundation</vendor>

      <description attribution="The OpenPKG Project">
The GNU Compiler Collection (GCC) provides a standard conforming and
highly portable ISO C, ISO C++, Fortran 90, and Java compiler.
      </description>

      <install-name>gcc64-44</install-name>

      <pkgname-base>TWWgcc64-44</pkgname-base>

      <version>4.4.6</version>
      <revision>1</revision>

      <replace pkgname-base="TWWgcc64-42"
        install-name="gcc64-42"/>
      <replace pkgname-base="TWWgcc64-343"
        install-name="gcc64-343"/>
    </package-manager>

