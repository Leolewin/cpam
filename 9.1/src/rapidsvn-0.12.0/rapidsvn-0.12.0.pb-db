<?xml version="1.0"?>
<packages>
  <package name="rapidsvn" version="0.12.0" revision="2">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>Qt based subversion frontend</title>
      <vendor>Julien Dumont</vendor>

      <description>
eSvn is a graphical frontend for the subversion revision system.
      </description>

      <install-name>rapidsvn012</install-name>

      <pkgname-base>TWWrapidsvn012</pkgname-base>

      <version>0.12.0</version>
      <revision>2</revision>

      <variable>
        <path name="GTKTHEMES_PATH"
          install-name="gtkthemes222"/>
        <path name="XDGDATA_DATADIR_PATH"
          install-name="xdgdata12">share</path>
      </variable>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibqt33"
          subpkg="runtime">v&gt;=3.3.8 r&gt;=3</depend>
        <depend pkgname-base="TWWwxgtk28"
          subpkg="runtime">v&gt;=2.8.12</depend>
        <depend pkgname-base="TWWsvn16"
          subpkg="runtime">v&gt;=1.6.17</depend>
      </subpkg>

      <replace pkgname-base="TWWrapidsvn09"
        install-name="rapidsvn09"/>

      <changelog>
        <change date="2012 January 30" revision="2"
        author="Peter O'Gorman" email="pogma@thewrittenword.com">
          <item><para>Update xdgdata dependency from 1.1 to
1.2</para></item>
          <item><para>Update wxGTK dependency from 2.8.10 to
2.8.12</para></item>
          <item><para>Update subversion dependency from 1.6.6 to
1.6.17</para></item>
        </change>
      </changelog>
    </package-manager>

