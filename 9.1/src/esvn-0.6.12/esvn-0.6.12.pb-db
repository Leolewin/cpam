        <change date="2009 December 8" revision="2"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Updated subversion dependency from 1.4.5 to
1.6.6</para></item>
        </change>
      </changelog>
    </package-manager>

