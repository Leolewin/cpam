<?xml version="1.0"?>
<packages>
  <package name="rcs" version="5.7" revision="4">
    <package-manager name="depot">
      <category>Development/Tools</category>
      <title>GNU RCS Revision Control System</title>
      <vendor>Free Software Foundation</vendor>

      <description>
The Revision Control System (RCS) manages multiple revisions of files.
RCS automates the storing, retrieval, logging, identification, and
merging of revisions. RCS is useful for text that is revised
frequently, including source code, programs, documentation, graphics,
papers, and form letters.
      </description>

      <install-name>rcs</install-name>

      <pkgname-base>TWWrcs</pkgname-base>

      <version>5.7</version>
      <revision>4</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWdiffutils32"
          subpkg="runtime">v&gt;=3.2</depend>
      </subpkg>

      <changelog>
        <change date="2012 January 28" revision="4"
        author="Gary V. Vaughan" email="gary@thewrittenword.com">
          <item><para>Update diffutils dependency from 2.8.1 to
3.2.</para></item>
        </change>

        <change date="2003 December 15" revision="3"
        author="Albert Chin-A-Young" email="china@thewrittenword.com">
          <item><para>Correct path to GNU diff3 binary</para></item>
        </change>
      </changelog>
    </package-manager>

