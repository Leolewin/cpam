        checksum="b80a5f777e419aa80a6ccf93a0e3d04a"
        checksum-type="md5" size="1965"/>

      <config>lib/greprefs/all.js</config>
      <config>lib/defaults/pref/all-thunderbird.js</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWcairo16"
          subpkg="runtime">v&gt;=1.6.4</depend>
        <depend pkgname-base="TWWgtk212"
          subpkg="runtime">v&gt;=2.12.11</depend>
        <depend pkgname-base="TWWlibidl08"
          subpkg="runtime">v&gt;=0.8.11</depend>
      </subpkg>

      <replace pkgname-base="TWWthunderbird15"
        install-name="thunderbird15"/>

