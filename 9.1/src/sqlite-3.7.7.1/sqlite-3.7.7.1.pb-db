        <depend pkgname-base="TWWlibreadline62"
          subpkg="runtime">v&gt;=6.2</depend>

        <postinstall path="pkg/add-to-include.paths-hpux"
          checksum="f9b17c15eeb48e1365cd16e0e6b5b9bd"
