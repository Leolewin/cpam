        link-dest="/sbin/init.d/TWWntp42"/>
      <init link-src="/sbin/rc1.d/K269TWWntp42"
        link-dest="/sbin/init.d/TWWntp42"/>

      <config>/sbin/init.d/TWWntp42</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/ntp.conf</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/ntp.keys</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWlibedit21"
          subpkg="runtime">v&gt;=2.11</depend>
        <depend pkgname-base="TWWlibopenssl10"
          subpkg="runtime">v&gt;=1.0.0e</depend>
        <depend pkgname-base="TWWperl5122"
          subpkg="runtime">v&gt;=5.12.2</depend>
      </subpkg>

      <replace pkgname-base="TWWntp412"
        install-name="ntp412"/>
    </package-manager>

