<?xml version="1.0"?>
<packages>
  <package name="fcpackage" version="2.8">
    <package-manager name="depot">
      <category>System Environment/Libraries</category>
      <title>Font configuration and rendering</title>
      <vendor>Keith Packard</vendor>

      <description>
Anti-alias rendering support for X-based applications using
fontconfig, Xft, and Xrender.
      </description>

      <install-name>fcpackage28</install-name>

      <pkgname-base>TWWfcpackage28</pkgname-base>

      <version>2.8</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWfonts11"
          subpkg="runtime">v&gt;=1.1</depend>
        <depend pkgname-base="TWWexpat20"
          subpkg="runtime">v&gt;=2.0.1</depend>
        <depend pkgname-base="TWWlibttf24"
          subpkg="runtime">v&gt;=2.4.6</depend>
      </subpkg>

      <replace pkgname-base="TWWfcp26"
        install-name="fcpackage26"/>
      <replace pkgname-base="TWWfcp24"
        install-name="fcpackage24"/>
    </package-manager>

