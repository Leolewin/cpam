<?xml version="1.0"?>
<packages>
  <package name="libxklavier" version="1.03">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>libxklavier XKB library</title>
      <vendor>Sergey V. Udaltsov</vendor>

      <description>
libxklavier - utility library to make XKB stuff easier
      </description>

      <install-name>libxklavier10</install-name>

      <pkgname-base>TWWlxk10</pkgname-base>

      <version>1.0.3</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWicn19"
          title="Iconv Library"
          subpkg="runtime">v&gt;=1.9.2</depend>
        <depend pkgname-base="TWWxml26"
          title="XML C Library"
          subpkg="runtime">v&gt;=2.6.11</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
