<?xml version="1.0"?>
<packages>
  <package name="lcms" version="1.12">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>LCMS Color Management Library</title>
      <vendor>Marti Maria</vendor>

      <description>
Little cms intends to be a small-footprint, speed optimized color
management engine.
      </description>

      <install-name>liblcms11</install-name>

      <pkgname-base>TWWlcm11</pkgname-base>

      <version>1.12</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWjpeg"
          title="JPEG Image Library"
          subpkg="runtime">v&gt;=6b</depend>
        <depend pkgname-base="TWWtif35"
          title="TIFF Image Library"
          subpkg="runtime">v&gt;=3.5.7</depend>
        <depend pkgname-base="TWWzlb11"
          title="ZLIB Compression Library"
          subpkg="runtime">v&gt;=1.1.4</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
