<?xml version="1.0"?>
<packages>
  <package name="dhcping" version="1.2">
    <package-manager name="pkgadd">
      <category>Applications/Internet</category>
      <title>DHCP Utility</title>
      <vendor>Edwin Groothuis</vendor>

      <description>
dhcping allows the system administrator to check if a remote DHCP
server is still functioning.
      </description>

      <install-name>dhcping12</install-name>

      <pkgname-base>TWWdhp12</pkgname-base>

      <version>1.2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
