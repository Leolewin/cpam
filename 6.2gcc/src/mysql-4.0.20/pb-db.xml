<?xml version="1.0"?>
<packages>
  <package name="mysql" version="4.0.20">
    <package-manager name="pkgadd">
      <category>Applications/Databases</category>
      <title>MySQL SQL Database Server</title>
      <vendor>T.C.X DataKonsult AB</vendor>

      <description attribution="The OpenPKG Project">
MySQL(TM) is a multi-user Relational Database Management System
(RDBMS), which is controlled through Structured Query Language (SQL)
operating in full multi-threading mode. The main goals of MySQL are
speed, robustness and ease of use.
      </description>

      <install-name>mysql4020</install-name>

      <pkgname-base>TWWm4020</pkgname-base>

      <version>4.0.20</version>
      <revision>1</revision>

      <init name="/etc/init.d/TWWmysql4020"
        path="pkg/init-sol"
        checksum="8b02a90eb1915b95424eddef06097572"
        checksum-type="md5" size="844"/>
      <init link-src="/etc/rc2.d/S79TWWmysql4020"
        link-dest="/etc/init.d/TWWmysql4020"/>
      <init link-src="/etc/rc1.d/K19TWWmysql4020"
        link-dest="/etc/init.d/TWWmysql4020"/>
      <init link-src="/etc/rc0.d/K19TWWmysql4020"
        link-dest="/etc/init.d/TWWmysql4020"/>

      <config>/etc/init.d/TWWmysql4020</config>
      <config>/etc${INSTALL_PREFIX}/my.cnf</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWncr54"
          title="Ncurses Curses Library"
          subpkg="runtime">v&gt;=5.4</depend>
        <depend pkgname-base="TWWpl582"
          title="Perl Programming Language"
          subpkg="runtime">v&gt;=5.8.2</depend>
        <depend pkgname-base="TWWrln43"
          title="Readline Library"
          subpkg="runtime">v&gt;=4.3</depend>
        <depend pkgname-base="TWWzlb11"
          title="ZLIB Compression Library"
          subpkg="runtime">v&gt;=1.1.4</depend>
      </subpkg>

      <replace pkgname-base="TWWm4015"
        install-name="mysql4015"/>
      <replace pkgname-base="TWWm4014"
        install-name="mysql4014"/>
    </package-manager>
  </package>
</packages>
