<?xml version="1.0"?>
<packages>
  <package name="gd" version="2.0.15" revision="1">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>GD Graphics Library</title>
      <vendor>Thomas Boutell</vendor>

      <description>
GD is a graphics library for creating PNG, JPEG, and WBMP images. It
allows your code to quickly draw images complete with lines, arcs,
text, multiple colors, cut and paste from other images, and flood
fills.
      </description>

      <install-name>libgd20</install-name>

      <pkgname-base>TWWlgd20</pkgname-base>

      <version>2.0.15</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWjpeg"
          title="JPEG Image Library"
          subpkg="runtime">v&gt;=6b</depend>
        <depend pkgname-base="TWWpl582"
          title="Perl Programming Language"
          subpkg="runtime">v&gt;=5.8.2</depend>
        <depend pkgname-base="TWWpng12"
          title="PNG Image Library"
          subpkg="runtime">v&gt;=1.2.4</depend>
        <depend pkgname-base="TWWttf21"
          title="FreeType Library"
          subpkg="runtime">v&gt;=2.1.4</depend>
        <depend pkgname-base="TWWxpm"
          title="XPM Image Library"
          subpkg="runtime">v&gt;=3.4k</depend>
        <depend pkgname-base="TWWzlb11"
          title="ZLIB Compression Library"
          subpkg="runtime">v&gt;=1.1.4</depend>
      </subpkg>

      <replace pkgname-base="TWWlgd18"
        install-name="libgd18"/>
      <replace pkgname-base="TWWlibgd"
        install-name="libgd"/>
    </package-manager>

    <changelog>
      <change date="2004 January 11" revision="1"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Upgrade Perl dependency to 5.8.2</para></item>
      </change>
    </changelog>
  </package>
</packages>
