<?xml version="1.0"?>
<packages>
  <package name="gtk-doc" version="1.1">
    <package-manager name="pkgadd">
      <category>Development/Tools</category>
      <title>API Documentation Generation Tool</title>
      <vendor>Damon Chaplin</vendor>

      <description>
GTK-Doc is used to document C code. It is typically used to document
the public API of libraries, such as the GTK+ and Gnome libraries.
      </description>

      <install-name>gtkdoc11</install-name>

      <pkgname-base>TWWgdc11</pkgname-base>

      <version>1.1</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWjad13"
          title="OpenJade DSSSL processor"
          subpkg="runtime">v&gt;=1.3.2</depend>
      </subpkg>

      <replace pkgname-base="TWWgdc10"
        install-name="gtkdoc10"/>
      <replace pkgname-base="TWWgtkdc"
        install-name="gtkdoc"/>
    </package-manager>
  </package>
</packages>
