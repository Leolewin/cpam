<?xml version="1.0"?>
<packages>
  <package name="zip" version="2.3">
    <package-manager name="pkgadd">
      <category>Applications/Archiving</category>
      <title>Zip Compression Utility</title>
      <vendor>Info-Zip Group</vendor>

      <description>
Zip 2.3 is a compression and file packaging utility.  It is compatible
with PKZIP 2.04g (Phil Katz ZIP) for MSDOS systems.
      </description>

      <install-name>zip</install-name>

      <pkgname-base>TWWzip</pkgname-base>

      <version>2.3</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
