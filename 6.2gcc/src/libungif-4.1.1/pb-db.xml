<?xml version="1.0"?>
<packages>
  <package name="libungif" version="4.1.1" revision="1">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>GIF Library</title>
      <vendor>Eric Raymond</vendor>

      <description attribution="FreeBSD Ports Collection">
GIF loading and saving shared library (saving uses an uncompressed gif
algorithm that does not use LZW compression).
      </description>

      <install-name>libungif</install-name>

      <pkgname-base>TWWlungf</pkgname-base>

      <version>4.1.1</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWpl582"
          title="Perl Programming Language"
          subpkg="runtime">v&gt;=5.8.2</depend>
      </subpkg>
    </package-manager>

    <changelog>
      <change date="2004 January 14" revision="1"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Upgrade Perl dependency to 5.8.2</para></item>
      </change>
    </changelog>
  </package>
</packages>
