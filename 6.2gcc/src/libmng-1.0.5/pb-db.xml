<?xml version="1.0"?>
<packages>
  <package name="libmng" version="1.0.5" revision="1">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>MNG Image Library</title>
      <vendor>Gerard Juyn</vendor>

      <description attribution="freshmeat.net">
libmng is the reference library for MNG, the animation extension to
the popular PNG format. It provides powerful animation features
combined with PNG's robustness and patent freedom.
      </description>

      <install-name>libmng10</install-name>

      <pkgname-base>TWWmng10</pkgname-base>

      <version>1.0.5</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWjpeg"
          title="JPEG Image Library"
          subpkg="runtime">v&gt;=6b</depend>
        <depend pkgname-base="TWWzlb11"
          title="ZLIB Compression Library"
          subpkg="runtime">v&gt;=1.1.4</depend>
      </subpkg>
    </package-manager>

    <changelog>
      <change date="2003 June 4" revision="1"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>SYSV style libraries on AIX</para></item>
      </change>
    </changelog>
  </package>
</packages>
