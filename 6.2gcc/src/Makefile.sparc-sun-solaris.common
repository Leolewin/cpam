#############################################################################
ifeq ($(upload),1)
sudo-1.6.7p5: /opt/TWWfsw/gcc332 \
 /opt/TWWfsw/sudo16 ${PDIST}/TWWsud16u sudo-1.6.7p5-gen-pkg-inst sudo-1.6.7p5-upload 
else
sudo-1.6.7p5: \
 /opt/TWWfsw/sudo16 ${PDIST}/TWWsud16u 
endif
#############################################################################
/opt/TWWfsw/sudo16: /opt/TWWfsw/gcc332
	${SB} ${SRCDIR}/sudo-1.6.7p5/sudo-1.6.7p5.sb
	${SB} -ip ${SRCDIR}/sudo-1.6.7p5/sudo-1.6.7p5.sb
${PDIST}/TWWsud16u:
	${PB} -1 ${SRCDIR}/sudo-1.6.7p5/sudo-1.6.7p5.pb
sudo-1.6.7p5-gen-pkg-inst: 
	${GPD}  ${SRCDIR}/sudo-1.6.7p5/sudo-1.6.7p5.pb >> ${PDIST}/pkg-db.xml
	(cd ${PDIST};zip  data.zip  TWWsud16* ;${M5S}  data.zip |awk '{print $$1}' > data.zip.md5)
	(cd ${PDIST};zip -r  sudo-1.6.7p5.pkgadd.pkg-inst pkg-db.xml data.zip data.zip.md5; ${M5S}  sudo-1.6.7p5.pkgadd.pkg-inst  |awk '{print $$1}' > sudo-1.6.7p5.pkgadd.pkg-inst.md5)
	cd ${PDIST};rm -f pkg-db.xml data.zip data.zip.md5

sudo-1.6.7p5-upload: 
	cd ${PDIST};${RSYNC} -azpv sudo-1.6.7p5.pkgadd.pkg-inst sudo-1.6.7p5.pkgadd.pkg-inst.md5 root@apps.comm.mot.com:${DEST}/${ARCHI}
	cd ${PDIST};rm -f ${PDIST}/pkg-db.xml sudo-1.6.7p5.pkgadd.pkg-inst sudo-1.6.7p5.pkgadd.pkg-inst.md5

sudo-1.6.7p5-clean: 
	rm -rf /opt/TWWfsw/sudo16* ${PDIST}/TWWsud16*
sudo-1.6.7p5-clean-packages: 
	rm -rf ${PDIST}/TWWsud16*


