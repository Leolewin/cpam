<?xml version="1.0"?>
<packages>
  <package name="atk" version="1.4.1">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>Accessibility Toolkit Library</title>
      <vendor>GNOME Accessibility Project</vendor>

      <description attribution="freshmeat.net">
Accessibility is enabling people with disabilities to participate in
substantial life activities that include work and the use of services,
products, and information. GNOME Accessibility is the suite of
software services and support in GNOME that allows people with
disabilities to utilize all of the functionality of the GNOME user
environment.
      </description>

      <install-name>libatk14</install-name>

      <pkgname-base>TWWatk14</pkgname-base>

      <version>1.4.1</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWglb22"
          title="Glib Library"
          subpkg="runtime">v&gt;=2.2.3</depend>
        <depend pkgname-base="TWWgt011"
          title="GNU Gettext"
          subpkg="runtime">v&gt;=0.11.4</depend>
        <depend pkgname-base="TWWicn18"
          title="Iconv Library"
          subpkg="runtime">v&gt;=1.8</depend>
      </subpkg>

      <replace pkgname-base="TWWatk12"
        install-name="libatk12"/>
      <replace pkgname-base="TWWatk10"
        install-name="libatk10"/>
    </package-manager>
  </package>
</packages>
