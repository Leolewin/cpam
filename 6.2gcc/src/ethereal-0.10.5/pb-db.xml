<?xml version="1.0"?>
<packages>
  <package name="ethereal" version="0.10.5">
    <package-manager name="pkgadd">
      <category>Applications/Internet</category>
      <title>Ethereal Network Protocol Analyzer</title>
      <vendor>Ethereal Development Team</vendor>

      <description>
Ethereal is a free network protocol analyzer for Unix and Windows. It
allows you to examine data from a live network or from a capture file
on disk. You can interactively browse the capture data, viewing
summary and detail information for each packet. Ethereal has several
powerful features, including a rich display filter language and the
ability to view the reconstructed stream of a TCP session.
      </description>

      <install-name>ethereal010</install-name>

      <pkgname-base>TWWeth0a</pkgname-base>

      <version>0.10.5</version>
      <revision>1</revision>

      <wrapper name="ethereal" path="pkg/ethereal-wrapper.c"
        checksum="463b42d686fe366302c0a3cbc2300fd8"
        checksum-type="md5" size="8420"/>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWgtk24"
          title="GTK+ Library"
          subpkg="runtime">v&gt;=2.4.4</depend>
        <depend pkgname-base="TWWpcr44"
          title="PCRE Perl regex Library"
          subpkg="runtime">v&gt;=4.4</depend>
        <depend pkgname-base="TWWsnp50"
          title="NET-SNMP SNMP Toolset"
          subpkg="runtime">v&gt;=5.0.9</depend>
        <depend pkgname-base="TWWzlb11"
          title="ZLIB Compression Library"
          subpkg="runtime">v&gt;=1.1.4</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
