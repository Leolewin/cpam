<?xml version="1.0"?>
<packages>
  <package name="gdbm" version="1.8.2">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>GNU DBM Library</title>
      <vendor>Jason Downs</vendor>

      <description>
GNU dbm is a set of database routines that use extendible hashing and
works similar to the standard UNIX dbm routines.
      </description>

      <install-name>libgdbm182</install-name>

      <pkgname-base>TWWdb182</pkgname-base>

      <version>1.8.2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
      </subpkg>

      <replace pkgname-base="TWWlgdbm18"
        install-name="libgdbm18"/>
      <replace pkgname-base="TWWlgdbm"
        install-name="libgdbm"/>
    </package-manager>
  </package>
</packages>
