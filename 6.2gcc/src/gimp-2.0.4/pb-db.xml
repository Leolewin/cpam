<?xml version="1.0"?>
<packages>
  <package name="gimp" version="2.0.4">
    <package-manager name="pkgadd">
      <category>Applications/Multimedia</category>
      <title>The GIMP GNU Image Manipulation Program</title>
      <vendor>GIMP Developer Group</vendor>

      <description>
The GIMP is designed to be a professional image manipulation program
suitable for such tasks as photo retouching, image composition and
image authoring.
      </description>

      <install-name>gimp20</install-name>

      <pkgname-base>TWWgmp20</pkgname-base>

      <version>2.0.4</version>
      <revision>1</revision>

      <wrapper name="gimp" path="pkg/gimp-wrapper.c"
        checksum="b044f5b0f2afdfea8818e14d66b7e4af"
        checksum-type="md5" size="8415"/>

      <config>etc/gimprc</config>
      <config>etc/gtkrc</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWart23"
          title="Libart 3D graphics library"
          subpkg="runtime">v&gt;=2.3.16</depend>
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWgp42"
          title="Printer Drivers"
          subpkg="runtime">v&gt;=4.2.7</depend>
        <depend pkgname-base="TWWgt014"
          title="GNU Gettext"
          subpkg="runtime">v&gt;=0.14.1</depend>
        <depend pkgname-base="TWWgtk24"
          title="GTK+ Library"
          subpkg="runtime">v&gt;=2.4.4</depend>
        <depend pkgname-base="TWWjpeg"
          title="JPEG Image Library"
          subpkg="runtime">v&gt;=6b</depend>
        <depend pkgname-base="TWWlcm11"
          title="LCMS Color Management Library"
          subpkg="runtime">v&gt;=1.12</depend>
        <depend pkgname-base="TWWlxf06"
          title="EXIF data handling library"
          systype="*-solaris2.[6789]*"
          subpkg="runtime">v&gt;=0.6.9</depend>
        <depend pkgname-base="TWWmng10"
          title="MNG Image Library"
          subpkg="runtime">v&gt;=1.0.5</depend>
        <depend pkgname-base="TWWpng12"
          title="PNG Image Library"
          subpkg="runtime">v&gt;=1.2.4</depend>
        <depend pkgname-base="TWWtif35"
          title="TIFF Image Library"
          subpkg="runtime">v&gt;=3.5.7</depend>
        <depend pkgname-base="TWWwmf02"
          title="WMF Graphics Library"
          subpkg="runtime">v&gt;=0.2.8</depend>
        <depend pkgname-base="TWWxpm"
          title="XPM Image Library"
          subpkg="runtime">v&gt;=3.4k</depend>
        <depend pkgname-base="TWWzlb11"
          title="ZLIB Compression Library"
          subpkg="runtime">v&gt;=1.1.4</depend>
      </subpkg>

      <replace pkgname-base="TWWgmp12"
        install-name="gimp12"/>
    </package-manager>
  </package>
</packages>
