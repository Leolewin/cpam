<?xml version="1.0"?>
<packages>
  <package name="t1lib" version="1.3.1">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>Type 1 Rasterizer Library</title>
      <vendor>Rainer Menzner</vendor>

      <description>
t1lib is a library written in the C programming language allowing a
programmer to generate bitmaps from Adobe(TM) Type 1 fonts quite
easily.
      </description>

      <install-name>t1lib13</install-name>

      <pkgname-base>TWWt1l13</pkgname-base>

      <version>1.3.1</version>
      <revision>1</revision>

      <config upgrade="1">share/t1lib.config</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
      </subpkg>

      <replace pkgname-base="TWWt1l12"
        install-name="t1lib12"/>
      <replace pkgname-base="TWWt1l11"
        install-name="t1lib11"/>
      <replace pkgname-base="TWWt1l10"
        install-name="t1lib10"/>
    </package-manager>
  </package>
</packages>
