<?xml version="1.0"?>
<packages>
  <package name="apache" version="1.3.31">
    <package-manager name="pkgadd">
      <category>System Environment/Daemons</category>
      <title>Apache Web Server</title>
      <vendor>The Apache Group</vendor>

      <description attribution="The OpenPKG Project">
The Apache Project is a collaborative software development effort
aimed at creating a robust, commercial-grade, featureful, and
freely-available source code implementation of an HTTP (Web) server.
      </description>

      <install-name>apache1331</install-name>

      <pkgname-base>TWWa1331</pkgname-base>

      <version>1.3.31</version>
      <revision>2</revision>

      <init name="/etc/init.d/TWWapache1331"
        path="pkg/init-sol"
        checksum="f92a9d8a5047f2bf608fa4163143a8b3"
        checksum-type="md5" size="1006"/>
      <init link-src="/etc/rc2.d/S79TWWapache1331"
        link-dest="/etc/init.d/TWWapache1331"/>
      <init link-src="/etc/rc1.d/K19TWWapache1331"
        link-dest="/etc/init.d/TWWapache1331"/>
      <init link-src="/etc/rc0.d/K19TWWapache1331"
        link-dest="/etc/init.d/TWWapache1331"/>

      <config>/etc/init.d/TWWapache1331</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/access.conf</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/httpd.conf</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/magic</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/mime.types</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/srm.conf</config>
      <config upgrade="1">/etc${INSTALL_PREFIX}/virtual.conf</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWexpat"
          title="XML parser Library"
          subpkg="runtime">v&gt;=1.95.6</depend>
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWldb42"
          title="Berkeley DB"
          subpkg="runtime">v&gt;=4.2.52</depend>
        <depend pkgname-base="TWWmm13"
          title="Shared memory Library"
          subpkg="runtime">v&gt;=1.3.1</depend>
        <depend pkgname-base="TWWpl582"
          title="Perl Interpreter"
          subpkg="runtime">v&gt;=5.8.2</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
