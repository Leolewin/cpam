<?xml version="1.0"?>
<packages>
  <package name="sed" version="4.0.9">
    <package-manager name="pkgadd">
      <category>Applications/Text</category>
      <title>GNU sed Stream Editor</title>
      <vendor>Free Software Foundation</vendor>

      <description>
GNU implementation of the POSIX stream editor 'sed'.
      </description>

      <install-name>gsed40</install-name>

      <pkgname-base>TWWgsd40</pkgname-base>

      <version>4.0.9</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWgt011"
          title="GNU Gettext"
          subpkg="runtime">v&gt;=0.11.4</depend>
        <depend pkgname-base="TWWicn18"
          title="Iconv Library"
          subpkg="runtime">v&gt;=1.8</depend>
      </subpkg>

      <replace pkgname-base="TWWgsed"
        install-name="gsed"/>
    </package-manager>
  </package>
</packages>
