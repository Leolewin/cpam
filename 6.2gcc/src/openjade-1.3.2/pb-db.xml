<?xml version="1.0"?>
<packages>
  <package name="openjade" version="1.3.2" revision="4">
    <package-manager name="pkgadd">
      <category>Applications/Publishing</category>
      <title>OpenJade DSSSL processor</title>
      <vendor>James Clark, Matthias Clasen</vendor>

      <description>
OpenJade is an implementation of DSSSL - Document Style Semantics and
Specification Language -- an ISO standard for formatting SGML (and
XML) documents.
      </description>

      <install-name>openjade13</install-name>

      <pkgname-base>TWWjad13</pkgname-base>

      <version>1.3.2</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWosp15"
          title="OpenSP SGML Parser"
          subpkg="runtime">v&gt;=1.5.1</depend>
        <depend pkgname-base="TWWxlc11"
          title="XML catalog files for Docbook"
          subpkg="runtime">v&gt;=1.1</depend>
      </subpkg>
    </package-manager>

    <changelog>
      <change date="2004 July 5" revision="4"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Update xmlcatalog dependency from 1.0 to
1.1</para></item>
        <item><para>Support HP-UX 10.20 by using GCC
3.2.3</para></item>
      </change>

      <change date="2004 January 3" revision="3"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Upgrade OpenSP dependency to 1.5.1</para></item>
      </change>

      <change date="2003 October 26" revision="2"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Upgrade GCC dependency to 3.3.2</para></item>
        <item><para>Upgrade OpenSP dependency to version that depends
on GCC 3.3.2</para></item>
      </change>

      <change date="2003 June 9" revision="1"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Fix path to XML catalog</para></item>
        <item><para>Add XML catalog to search path</para></item>
      </change>
    </changelog>
  </package>
</packages>
