<?xml version="1.0"?>
<packages>
  <package name="libexif" version="0.6.9">
    <package-manager name="pkgadd">
      <category>System Environment/Libraries</category>
      <title>EXIF data handling library</title>
      <vendor>Lutz Mueller</vendor>

      <description>
libexif is a library for parsing, editing, and saving EXIF data.
      </description>

      <install-name>libexif06</install-name>

      <pkgname-base>TWWlxf06</pkgname-base>

      <version>0.6.9</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWgt014"
          title="GNU Gettext"
          subpkg="runtime">v&gt;=0.14.1</depend>
        <depend pkgname-base="TWWicn19"
          title="Iconv Library"
          subpkg="runtime">v&gt;=1.9.2</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
