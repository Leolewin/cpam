<?xml version="1.0"?>
<packages>
  <package name="wsdd-apache" version="1.3.31">
    <package-manager name="pkgadd">
      <category>System Environment/Daemons</category>
      <title>Apache Web Server WSDD customization</title>
      <vendor>EIT/WSDD/CGISS Mororola Inc.</vendor>

      <description >
This is a WSDD customization package for apache-1.3.31 
      </description>

      <install-name>wsdd-apache1331</install-name>

      <pkgname-base>TWa1331S</pkgname-base>

      <version>1.3.31</version>
      <revision>2</revision>

      <subpkg type="runtime">
        <postinstall path="pkg/postinstall-sol"/>
        <depend pkgname-base="TWWa1331"
          title="Apache 1.3.31" subpkg="runtime">v&gt;=1.3.31 </depend>
        <depend pkgname-base="TWWa1331"
          title="Apache 1.3.31" subpkg="conf">v&gt;=1.3.31 </depend>
        <depend pkgname-base="TWWa1331"
          title="Apache 1.3.31" subpkg="man">v&gt;=1.3.31 </depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
