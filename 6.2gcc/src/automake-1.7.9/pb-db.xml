<?xml version="1.0"?>
<packages>
  <package name="automake" version="1.7.9">
    <package-manager name="pkgadd">
      <category>Development/Tools</category>
      <title>GNU Automake</title>
      <vendor>Tom Tromey</vendor>

      <description>
Automake is a tool for automatically generating `Makefile.in's from
files called `Makefile.am'. The generated `Makefile.in's are compliant
with the GNU Makefile standards.
      </description>

      <install-name>automake17</install-name>

      <pkgname-base>TWWamk17</pkgname-base>

      <version>1.7.9</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWpl582"
          title="Perl Programming Language"
          subpkg="runtime">v&gt;=5.8.2</depend>
      </subpkg>

      <replace pkgname-base="TWWamk16"
        install-name="automake16"/>
      <replace pkgname-base="TWWamk15"
        install-name="automake15"/>
      <replace pkgname-base="TWWamake"
        install-name="automake"/>
    </package-manager>
  </package>
</packages>
