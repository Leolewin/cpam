<?xml version="1.0"?>
<packages>
  <package name="tetex" version="2.0.2" revision="3">
    <package-manager name="pkgadd">
      <category>Applications/Publishing</category>
      <title>teTeX TeX distribution</title>
      <vendor>Thomas Esser</vendor>

      <description attribution="The OpenPKG Project">
teTeX is a complete TeX distribution for Unix compatible systems,
maintained by Thomas Esser. It is based on Karl Berry's original Web2c
distribution and contains LaTeX, pdfTeX, e-TeX, Omega, xdvik, dvipsk,
dviljk, dvipdfm, ps2pk, makeindex, texconfig, updmap, and texdoctk.
      </description>

      <install-name>tetex20</install-name>

      <pkgname-base>TWWtex20</pkgname-base>

      <version>2.0.2</version>
      <revision>1</revision>
<!--
      <config>texmf/dvips/config/config.ps</config>
      <config>texmf/dvips/config/builtin35.map</config>
      <config>texmf/dvips/config/download35.map</config>
      <config>texmf/dvips/config/dvipdfm_dl14.map</config>
      <config>texmf/dvips/config/dvipdfm_ndl14.map</config>
      <config>texmf/dvips/config/pdftex_dl14.map</config>
      <config>texmf/dvips/config/pdftex_ndl14.map</config>
      <config>texmf/dvips/config/ps2pk.map</config>
      <config>texmf/dvips/config/psfonts_pk.map</config>
      <config>texmf/dvips/config/psfonts_t1.map</config>
-->
      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWgs70"
          title="GNU Ghostscript"
          subpkg="runtime">v&gt;=7.07</depend>
        <depend pkgname-base="TWWpl582"
          title="Perl Programming Language"
          subpkg="runtime">v&gt;=5.8.2</depend>
        <depend pkgname-base="TWWpng12"
          title="PNG Image Library"
          subpkg="runtime">v&gt;=1.2.4</depend>
        <depend pkgname-base="TWWt1l13"
          title="Type 1 Rasterizer Library"
          subpkg="runtime">v&gt;=1.3.1</depend>
        <depend pkgname-base="TWWzlb11"
          title="ZLIB Compression Library"
          subpkg="runtime">v&gt;=1.1.4</depend>
      </subpkg>

      <replace pkgname-base="TWWtetex"
        install-name="tetex"/>
    </package-manager>

    <changelog>
      <change date="2004 March 11" revision="3"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Add beamer-2.10, memoir-1.7, pgf-0.60, and
xcolor-1.09 packages</para></item>
      </change>

      <change date="2004 January 13" revision="2"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Upgrade Perl dependency to 5.8.2</para></item>
        <item><para>Upgrade Ghostscript dependency to
7.07</para></item>
      </change>

      <change date="2003 November 11" revision="1"
      author="Albert Chin-A-Young" email="china@thewrittenword.com">
        <item><para>Upgrade GCC dependency to 3.3.2</para></item>
      </change>
    </changelog>
  </package>
</packages>
