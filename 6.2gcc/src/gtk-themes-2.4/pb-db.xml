<?xml version="1.0"?>
<packages>
  <package name="gtk-themes" version="2.4">
    <package-manager name="pkgadd">
      <category>Applications/Multimedia</category>
      <title>GTK+ 2.4 Themes</title>
      <vendor>3rd Parties</vendor>

      <description>
Themes for use with GTK+ 2.4.x: Metal, Redmond95, ThinIce, XFce.
      </description>

      <install-name>gtkthemes24</install-name>

      <pkgname-base>TWWgtt24</pkgname-base>

      <version>2.4.0</version>
      <revision>1</revision>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWgtk24"
          title="GTK+ Library"
          subpkg="runtime">v&gt;=2.4.4</depend>
      </subpkg>
    </package-manager>
  </package>
</packages>
