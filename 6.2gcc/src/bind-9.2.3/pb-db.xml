<?xml version="1.0"?>
<packages>
  <package name="bind" version="9.2.3">
    <package-manager name="pkgadd">
      <category>System Environment/Daemons</category>
      <title>BIND Domain Name Server</title>
      <vendor>Internet Software Consortium</vendor>

      <description attribution="The OpenPKG Project">
Berkeley Internet Name Domain (BIND) is an implementation of the
Domain Name System (DNS) protocols and provides an openly
redistributable reference implementation of the major components of
the Domain Name System, including a Domain Name System server (named).
      </description>

      <install-name>bind923</install-name>

      <pkgname-base>TWWbn923</pkgname-base>

      <version>9.2.3</version>
      <revision>1</revision>

      <init name="/etc/init.d/TWWbind923"
        path="pkg/bind-init-sol"
        checksum="301c36de0c2c54b139d080f907ae8fc6"
        checksum-type="md5" size="994"/>
      <init link-src="/etc/rc2.d/S79TWWbind923"
        link-dest="/etc/init.d/TWWbind923"/>
      <init link-src="/etc/rc1.d/K19TWWbind923"
        link-dest="/etc/init.d/TWWbind923"/>
      <init link-src="/etc/rc0.d/K19TWWbind923"
        link-dest="/etc/init.d/TWWbind923"/>

      <config>/etc/init.d/TWWbind923</config>
      <config>/etc${INSTALL_PREFIX}/db.127.0.0</config>
      <config>/etc${INSTALL_PREFIX}/db.cache</config>
      <config>/etc${INSTALL_PREFIX}/lwresd.conf</config>
      <config>/etc${INSTALL_PREFIX}/named.conf</config>
      <config>/etc${INSTALL_PREFIX}/rndc.conf</config>
      <config>/etc${INSTALL_PREFIX}/rndc.key</config>

      <subpkg type="runtime">
        <depend pkgname-base="TWWgc332"
          title="GNU Compiler Collection"
          subpkg="libruntime">v&gt;=3.3.2</depend>
        <depend pkgname-base="TWWss097"
          title="OpenSSL SSL/TLS Crypto Toolkit"
          subpkg="runtime">v&gt;=0.9.7b</depend>
      </subpkg>

      <replace pkgname-base="TWWbn922"
        install-name="bind922"/>
      <replace pkgname-base="TWWbn921"
        install-name="bind921"/>
      <replace pkgname-base="TWWbn833"
        install-name="bind833"/>
    </package-manager>
  </package>
</packages>
